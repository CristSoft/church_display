{
  "title": "Himno #230 Abre tu corazón",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Abre tu corazón, abre tu corazón."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Jesús entrará, y él te salvará",
        "si abres tu corazón."
      ]
    },
    "3": {
      "verse": "2",
      "text": [
        "Abro mi corazón, abro mi corazón."
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "Ven, entra, Jesús; dame gozo y luz,",
        "te abro mi corazón."
      ]
    }
  }
}