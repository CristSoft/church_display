{
  "title": "Himno #285 Confío en Jesucristo",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Confío en Jesucristo,",
        "quien en la cruz murió."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Por esa muerte invicto",
        "al cielo marcho yo."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "Con sangre tan preciosa",
        "mis culpas lava él:",
        "la derramó copiosa"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "por mí ya Emanuel."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Me cubre tu justicia",
        "de plena protección;",
        "tú eres mi delicia,"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "mi eterna salvación.",
        "Jesús, en ti descanso;",
        "reposo tú me das;"
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "tranquilo, pues, avanzo",
        "al cielo, donde estás."
      ]
    },
    "8": {
      "verse": "3",
      "text": [
        "A disfrutar invitas.",
        "Acepto, mi Señor,"
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "delicias infinitas y celestial amor.",
        "Espero al fin mirarte,"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "oír tu dulce voz;",
        "espero yo cantarte,",
        "mi Salvador, mi Dios."
      ]
    }
  }
}