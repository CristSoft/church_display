{
  "title": "Himno #62 Santo, Santo, Santo, Santo es el Señor",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Santo, Santo, Santo,",
        "Santo es el Señor.",
        "Santo, Santo, Santo,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Santo es nuestro Dios.",
        "Todopoderoso",
        "siempre reinará."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "Su dominio eterno",
        "nunca pasará."
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "Santo, Santo, Santo,",
        "Santo es el Señor.",
        "Santo, Santo, Santo,"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Santo es nuestro Dios.",
        "Digno es de gloria,",
        "honra y poder."
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "Su grandiosa obra",
        "brilla por doquier."
      ]
    }
  }
}