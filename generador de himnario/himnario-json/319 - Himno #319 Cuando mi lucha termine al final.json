{
  "title": "Himno #319 Cuando mi lucha termine al final",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Cuando mi lucha termine al final"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "y me halle salvo en la playa eternal,"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "junto al que adoro, mi Rey celestial,"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "eterna gloria será para mí."
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "Gloria sin fin eso será,",
        "gloria sin fin eso será."
      ]
    },
    "6": {
      "verse": "coro",
      "text": [
        "Cuando por gracia su faz vea allí,"
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "eterna gloria será para mí."
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "Cuando la gracia infinita me dé"
      ]
    },
    "9": {
      "verse": "2",
      "text": [
        "bella morada en la casa del Rey,",
        "maravillado su rostro veré,"
      ]
    },
    "10": {
      "verse": "2",
      "text": [
        "y eterna gloria será para mí."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Gloria sin fin eso será,",
        "gloria sin fin eso será."
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "Cuando por gracia su faz vea allí,"
      ]
    },
    "13": {
      "verse": "coro",
      "text": [
        "eterna gloria será para mí."
      ]
    },
    "14": {
      "verse": "3",
      "text": [
        "Encontraré a mis amados allá;",
        "gozo cual río en redor correrá;"
      ]
    },
    "15": {
      "verse": "3",
      "text": [
        "dulce sonrisa Jesús me dará,",
        "eterna gloria será para mí."
      ]
    },
    "16": {
      "verse": "coro",
      "text": [
        "Gloria sin fin eso será,",
        "gloria sin fin eso será."
      ]
    },
    "17": {
