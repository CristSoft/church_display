{
  "title": "Himno #246 Te quiero, mi Señor",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Te quiero, mi Señor;",
        "habita en mí,",
        "y vencedor seré"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "por fe en ti."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Te quiero, sí, te quiero;",
        "siempre te anhelo;"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "bendíceme, te ruego;",
        "acudo a ti."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Te quiero, ¡oh Jesús!,",
        "mi Salvador.",
        "¡Oh!, hazme en verdad"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "tu servidor."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Te quiero, sí, te quiero;",
        "siempre te anhelo;"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "bendíceme, te ruego;",
        "acudo a ti."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Tu voluntad, Señor,",
        "enséñame;",
        "y de tu gran amor,",
        "¡oh!, cólmame."
      ]
    },
    "10": {
      "verse": "coro",
      "text": [
        "Te quiero, sí, te quiero;",
        "siempre te anhelo;"
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "bendíceme, te ruego;",
        "acudo a ti."
      ]
    },
    "12": {
      "verse": "4",
      "text": [
        "¡Oh! mi gran Bienhechor,",
        "en tentación",
        "concédeme valor"
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "y protección."
      ]
    },
    "14": {
      "verse": "coro",
      "text": [
        "Te quiero, sí, te quiero;",
        "siempre te anhelo;"
      ]
