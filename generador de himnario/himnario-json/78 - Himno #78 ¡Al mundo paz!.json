{
  "title": "Himno #78 ¡Al mundo paz!",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "¡Al mundo paz, nació Jesús,",
        "nació ya nuestro Rey!"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "El corazón ya tiene luz,",
        "y paz su santa grey,"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "y paz su santa grey,",
        "y paz, y paz, su santa grey."
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "¡Al mundo paz; el Salvador,",
        "supremo reinará!"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Ya es feliz el pecador:",
        "Jesús perdón le da,"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "Jesús perdón le da,",
        "Jesús, Jesús perdón le da."
      ]
    },
    "7": {
      "verse": "3",
      "text": [
        "Al mundo él gobernará",
        "con gracia y con poder;"
      ]
    },
    "8": {
      "verse": "3",
      "text": [
        "y a toda nación demostrará",
        "su amor y su poder,"
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "su amor y su poder,",
        "su amor, su amor y su poder."
      ]
    }
  }
}