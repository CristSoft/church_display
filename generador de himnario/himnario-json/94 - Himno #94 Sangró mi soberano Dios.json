{
  "title": "Himno #94 Sangró mi soberano Dios",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Sangró mi soberano Dios,",
        "murió mi Salvador."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Su vida quiso entregar",
        "por mí, tan pecador."
      ]
    },
    "3": {
      "verse": "2",
      "text": [
        "Y yo, contrito, al ver su cruz,",
        "el rostro encubriré;"
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "con lágrimas de gratitud",
        "mi alma inundaré."
      ]
    },
    "5": {
      "verse": "3",
      "text": [
        "Mas no con llanto pagaré",
        "la deuda de su amor."
      ]
    },
    "6": {
      "verse": "3",
      "text": [
        "Mi ser entero yo te doy,",
        "¡no puedo más, Señor!"
      ]
    },
    "7": {
      "verse": "4",
      "text": [
        "¡Oh, Salvador, ayúdame;",
        "que sea fiel a ti;"
      ]
    },
    "8": {
      "verse": "4",
      "text": [
        "y cuando en gloria reines tú,",
        "acuérdate de mí!"
      ]
    }
  }
}