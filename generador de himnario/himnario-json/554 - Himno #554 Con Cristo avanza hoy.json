{
  "title": "Himno #554 Con Cristo avanza hoy",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Con Cristo avanza hoy,",
        "él te llamó a ti.",
        "Te ha guiado"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "y te guardará en su amor.",
        "Él cumplirá fielmente lo que a"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "su iglesia prometió."
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "Con Cristo avanza hoy,",
        "resuelto a alcanzar"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "victorias en su nombre",
        "y por su poder.",
        "Te guardará,"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "su Santo Espíritu",
        "te fortalecerá."
      ]
    },
    "7": {
      "verse": "3",
      "text": [
        "Con Cristo avanza hoy,",
        "las buenas nuevas da;",
        "que por su sangre"
      ]
    },
    "8": {
      "verse": "3",
      "text": [
        "salvación ofrece ya.",
        "Avanza hoy,",
        "imita al Salvador,"
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "con Cristo avanza hoy."
      ]
    }
  }
}