{
  "title": "Himno #347 Con gozo canto al Señor",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Con gozo canto al Señor",
        "desde que salvo soy,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "pues es mi Rey, mi Salvador,",
        "desde que salvo soy."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Desde que salvo soy, desde que salvo soy"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "solo en él me gloriaré; desde que salvo soy"
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "en mi Redentor me gloriaré."
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "Yo amo a Dios por su bondad,",
        "desde que salvo soy,"
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "y quiero hacer su voluntad,",
        "desde que salvo soy."
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "Desde que salvo soy, desde que salvo soy"
      ]
    },
    "9": {
      "verse": "coro",
      "text": [
        "solo en él me gloriaré; desde que salvo soy"
      ]
    },
    "10": {
