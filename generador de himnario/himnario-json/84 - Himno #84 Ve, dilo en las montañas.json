{
  "title": "Himno #84 Ve, dilo en las montañas",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Pastores muy piadosos,",
        "con gran admiración,",
        "oyeron, jubilosos,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "del don de Salvación."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Ve, dilo en las montañas,",
        "en todas partes, por doquier."
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "Ve, dilo en las montañas:",
        "que Cristo, el Rey, nació."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "En esa noche oscura",
        "los alumbró gran luz;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "y luego, con premura,",
        "buscaron a Jesús."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Ve, dilo en las montañas,",
        "en todas partes, por doquier."
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "Ve, dilo en las montañas:",
        "que Cristo, el Rey, nació."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Doquiera que estemos",
        "debemos proclamar"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "que, en Cristo, hoy podemos",
        "la redención gozar."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Ve, dilo en las montañas,",
        "en todas partes, por doquier."
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "Ve, dilo en las montañas:",
        "que Cristo, el Rey, nació."
      ]
    }
  }
}