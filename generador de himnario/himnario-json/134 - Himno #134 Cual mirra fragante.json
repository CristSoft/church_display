{
  "title": "Himno #134 Cual mirra fragante",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Cual mirra fragante que exhala su olor"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "y ricos perfumes esparce en redor,"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "tu nombre, ¡oh Amado!, a mi corazón"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "lo llena de gozo, transpórtalo a Sion."
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "Aleluya, aleluya",
        "al Cordero de Dios;",
        "aleluya al Amado,"
      ]
    },
    "6": {
      "verse": "coro",
      "text": [
        "al bendito Jesús."
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "Cual voz amigable que al triste viador"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "en bosque perdido le inspira valor,"
      ]
    },
    "9": {
      "verse": "2",
      "text": [
        "tu nombre me anima y me hace saber"
      ]
    },
    "10": {
      "verse": "2",
      "text": [
        "que ofreces, piadoso, rescate a mi ser."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Aleluya, aleluya",
        "al Cordero de Dios;",
        "aleluya al Amado,"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "al bendito Jesús."
      ]
    },
    "13": {
      "verse": "3",
      "text": [
        "Cual luz que, brillando del alto fanal,"
      ]
    },
    "14": {
      "verse": "3",
      "text": [
        "al nauta en la noche señala el canal,"
      ]
    },
    "15": {
      "verse": "3",
      "text": [
        "tu nombre, esparciendo benéfica luz,"
      ]
    },
    "16": {
      "verse": "3",
      "text": [
        "al cielo me lleva, bendito Jesús."
      ]
    },
    "17": {
      "verse": "coro",
      "text": [
        "Aleluya, aleluya",
        "al Cordero de Dios;",
        "aleluya al Amado,"
      ]
    },
    "18": {
      "verse": "coro",
      "text": [
        "al bendito Jesús."
      ]
    }
  }
}