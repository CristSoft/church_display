{
  "title": "Himno #194 Vive en mí",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Vive, Santo Espíritu,",
        "en este pobre ser;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "y que tu fuego celestial",
        "transforme mi existir."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Vive en mí, vive en mí,",
        "Santo y fiel Consolador."
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "Limpia del mal mi corazón;",
        "llénalo siempre de tu amor."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Habla, Santo Espíritu,",
        "háblame con poder;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "y al escuchar tu dulce voz,",
        "consagre a ti mi ser."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Vive en mí, vive en mí,",
        "Santo y fiel Consolador."
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "Limpia del mal mi corazón;",
        "llénalo siempre de tu amor."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Santo, Santo Espíritu,",
        "lléname de fervor,"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "para servirte con amor,",
        "y sea yo vencedor."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Vive en mí, vive en mí,",
        "Santo y fiel Consolador."
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "Limpia del mal mi corazón;",
        "llénalo siempre de tu amor."
      ]
    }
  }
}