{
  "title": "Himno #192 Llena mi ser",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Santo Espíritu de Dios,",
        "llena tú mi ser."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Santo Espíritu de Dios,",
        "dame tu poder.",
        "Toma mi ser"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "para amoldarlo.",
        "Santo Espíritu de Dios,",
        "llena tú mi ser."
      ]
    }
  }
}