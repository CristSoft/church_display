{
  "title": "Himno #602 Es el amor divino",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Es el amor divino",
        "mi gozo y mi placer,",
        "allana mi camino"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "y me hace obedecer."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Dios es amor,",
        "soy su pequeñuelo;",
        "quiero ser santo",
        "como es él."
      ]
    },
    "4": {
      "verse": "coro",
