{
  "title": "Himno #397 Oh buen Señor, velada está",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Oh buen Señor, velada está",
        "tu santa forma aquí,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "pues el pecado, oh Jehová,",
        "te esconde hoy de mí."
      ]
    },
    "3": {
      "verse": "2",
      "text": [
        "Aunque no te distingo hoy,",
        "sé que conmigo estás;"
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "y si en afán o lucha estoy,",
        "tu auxilio me darás."
      ]
    },
    "5": {
      "verse": "3",
      "text": [
        "Como un sueño nocturnal,",
        "dorada ensoñación,"
      ]
    },
    "6": {
      "verse": "3",
      "text": [
        "es tu presencia sin igual",
        "la más feliz visión."
      ]
    },
    "7": {
      "verse": "4",
      "text": [
        "Tu forma puedo percibir",
        "en alas de la fe;"
      ]
    },
    "8": {
      "verse": "4",
      "text": [
        "con tu influencia en mi vivir,",
        "confiado andaré."
      ]
    }
  }
}