{
  "title": "Himno #7 Oh Dios, mi soberano Rey",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Oh Dios, mi soberano Rey;",
        "a ti daré loor;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "tu nombre yo exaltaré,",
        "santísimo Señor."
      ]
    },
    "3": {
      "verse": "2",
      "text": [
        "Tus obras evidencia son",
        "de tu infinito amor,"
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "y cantan con alegre voz",
        "las glorias del Señor."
      ]
    },
    "5": {
      "verse": "3",
      "text": [
        "Aquel que busca salvación,",
        "en Cristo la hallará;"
      ]
    },
    "6": {
      "verse": "3",
      "text": [
        "y su ferviente petición",
        "él pronto atenderá."
      ]
    },
    "7": {
      "verse": "4",
      "text": [
        "Eternamente durará",
        "el reino del Señor."
      ]
    },
    "8": {
      "verse": "4",
      "text": [
        "Allí sus siervos gozarán",
        "la plenitud de amor."
      ]
    }
  }
}