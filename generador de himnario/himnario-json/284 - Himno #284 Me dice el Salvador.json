{
  "title": "Himno #284 Me dice el Salvador",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Me dice el Salvador:",
        "\"Es poco tu poder;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "hijo débil, halla en mí",
        "todo cuanto has menester\"."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Todo debo a él, pues ya lo pagó;"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "de las manchas del pecar cual nieve me lavó."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "He hallado, mi Señor,",
        "que solo tu poder",
        "a mi duro corazón"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "es capaz de enternecer."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Todo debo a él, pues ya lo pagó;"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "de las manchas del pecar cual nieve me lavó."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Nada bueno hay en mí:",
        "tu gracia buscaré,"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "y en la sangre de Jesús",
        "mis pecados lavaré."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Todo debo a él, pues ya lo pagó;"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "de las manchas del pecar cual nieve me lavó."
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "Cuando ante el trono, allá,",
        "completo en él esté,"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "a los pies de mi Jesús",
        "mi tributo ofreceré."
      ]
    },
    "15": {
      "verse": "coro",
      "text": [
        "Todo debo a él, pues ya lo pagó;"
      ]
    },
    "16": {
      "verse": "coro",
      "text": [
        "de las manchas del pecar cual nieve me lavó."
      ]
    }
  }
}