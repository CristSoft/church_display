{
  "title": "Himno #30 Abre mis ojos",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Abre mis ojos,",
        "Jesús, quiero verte,",
        "conmigo tenerte,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "decir que te amo.",
        "Abre mi oído,",
        "hoy quiero escucharte."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "¡Abre mis ojos",
        "para contemplarte!"
      ]
    }
  }
}