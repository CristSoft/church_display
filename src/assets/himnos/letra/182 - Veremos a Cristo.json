{
  "title": "Veremos a Cristo",
  "number": "182",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "El cielo se abrirá y le dará paso;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "millones de estrellas lo alabarán."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "Su mirada radiante nos dará santo gozo"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "y contemplaremos al Salvador."
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "Veremos a Cristo, al Rey veremos"
      ]
    },
    "6": {
      "verse": "coro",
      "text": [
        "cara acara en toda su gloria."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Veremos a Cristo, al Rey veremos",
        "cara a cara, al Rey y Señor."
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "Trompeta se oirá llamando a los muertos;"
      ]
    },
    "9": {
      "verse": "2",
      "text": [
        "del sueño despertarán, a vida eternal."
      ]
    },
    "10": {
      "verse": "2",
      "text": [
        "Y los que estemos vivos,",
        "transformados seremos"
      ]
    },
    "11": {
      "verse": "2",
      "text": [
        "y contemplaremos al Salvador."
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "Veremos a Cristo, al Rey veremos"
      ]
    },
    "13": {
