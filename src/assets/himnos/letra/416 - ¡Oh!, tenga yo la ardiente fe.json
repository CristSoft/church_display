{
  "title": "Himno #416 ¡Oh!, tenga yo la ardiente fe",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "¡Oh!, tenga yo la ardiente fe",
        "que ante enemigos mil"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "no temblará ni aun cuando esté,",
        "ni aun cuando esté"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "en la miseria vil,",
        "en la miseria vil."
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "Tal fe, que no se quejará",
        "ni bajo corrección,"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "en hora amarga afirmará,",
        "afirmará",
        "en Dios el corazón,"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "en Dios el corazón."
      ]
    },
    "7": {
      "verse": "3",
      "text": [
        "Refulge más si hay tempestad",
        "rugiendo en derredor,"
      ]
    },
    "8": {
      "verse": "3",
      "text": [
        "y cruzará la oscuridad,",
        "la oscuridad",
        "sin dudas ni temor,"
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "sin dudas ni temor."
      ]
    },
    "10": {
      "verse": "4",
      "text": [
        "Oh, dame a mí, Señor, tal fe,",
        "y, venga bien o mal,"
      ]
    },
    "11": {
      "verse": "4",
      "text": [
        "estando aquí gustar podré,",
        "gustar podré",
        "la dicha celestial,"
      ]
    },
    "12": {
      "verse": "4",
      "text": [
        "la dicha celestial."
      ]
    }
  }
}