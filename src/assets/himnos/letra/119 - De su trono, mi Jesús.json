{
  "title": "De su trono, mi Jesús",
  "number": "119",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "De su trono, mi Jesús,",
        "a morir aquí bajó,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "y clavado en la cruz,",
        "mis pecados él pagó."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Sí, Cristo me ama;",
        "sí, Cristo me ama;",
        "sí, Cristo me ama,"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "la Biblia dice así."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Bien me quiere el Salvador",
        "que sufrió por mi maldad."
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "¡Te bendigo, mi Señor,",
        "reconozco tu bondad!"
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Sí, Cristo me ama;",
        "sí, Cristo me ama;",
        "sí, Cristo me ama,"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "la Biblia dice así."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Guarda fiel mi corazón",
        "tú, que velas sobre mí;"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "y con toda devoción",
        "haz que viva yo por ti."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Sí, Cristo me ama;",
        "sí, Cristo me ama;",
        "sí, Cristo me ama,"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "la Biblia dice así."
      ]
    }
  }
}