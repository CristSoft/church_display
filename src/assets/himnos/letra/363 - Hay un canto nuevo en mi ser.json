{
  "title": "Hay un canto nuevo en mi ser",
  "number": "363",
  "verses": "4",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Hay un canto nuevo en mi ser,",
        "es la voz de mi Jesús;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "que me infunde dulce paz y amor,",
        "al andar yo en su luz."
      ]
    },
    "3": {
      "verse": "coro 1",
      "text": [
        "Cristo, Cristo, Cristo,",
        "nombre sin igual;"
      ]
    },
    "4": {
      "verse": "coro 1",
      "text": [
        "llene siempre mi alma",
        "esa nota celestial."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Él me da su gracia celestial,",
        "al cumplir su voluntad;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "de su trono fluyen cual raudal,",
        "las riquezas de bondad."
      ]
    },
    "7": {
      "verse": "coro 2",
      "text": [
        "Cristo, Cristo, Cristo,",
        "nombre sin igual;"
      ]
    },
    "8": {
      "verse": "coro 2",
      "text": [
        "llene siempre mi alma",
        "esa nota celestial."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Muchas pruebas debo soportar",
        "pero Cristo me guiará;"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "y a mí por fe me toca andar,",
        "su amor me sostendrá."
      ]
    },
    "11": {
      "verse": "coro 3",
      "text": [
        "Cristo, Cristo, Cristo,",
        "nombre sin igual;"
      ]
    },
    "12": {
      "verse": "coro 3",
      "text": [
        "llene siempre mi alma",
        "esa nota celestial."
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "Cristo en las nubes volverá,",
        "a llevarme al hogar;"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "para siempre mi alma gozará,",
        "porque en Cristo supe fiar."
      ]
    },
    "15": {
      "verse": "coro 4",
      "text": [
        "Cristo, Cristo, Cristo,",
        "nombre sin igual;"
      ]
    },
    "16": {
      "verse": "coro 4",
      "text": [
        "llene siempre mi alma",
        "esa nota celestial."
      ]
    }
  }
}