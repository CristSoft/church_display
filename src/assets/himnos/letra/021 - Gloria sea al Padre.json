{
  "title": "Gloria sea al Padre",
  "number": "21",
  "verses": "1",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Gloria sea al Padre",
        "y al Hijo Dios",
        "y al Santo Espíritu."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Como eran al principio,",
        "son hoy y serán por siempre."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "¡Gloria sin fin!",
        "Amén, amén."
      ]
    }
  }
}