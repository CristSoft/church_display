{
  "title": "Más de Jesús",
  "number": "445",
  "verses": "4",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Más de Jesús deseo saber,",
        "más de su gracia conocer,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "más de su salvación gozar,",
        "más de su dulce amor gustar."
      ]
    },
    "3": {
      "verse": "coro 1",
      "text": [
        "Más quiero amarle,",
        "más quiero honrarle,"
      ]
    },
    "4": {
      "verse": "coro 1",
      "text": [
        "más de su salvación gozar,",
        "más de su dulce amor gustar."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Más de Jesús deseo oír,",
        "más de su santa Ley cumplir,"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "más de su voluntad saber,",
        "más de su Espíritu tener."
      ]
    },
    "7": {
