{
  "title": "Un hombre llegose de noche a Jesús",
  "number": "225",
  "verses": "4",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Un hombre llegose de noche a Jesús,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "buscando la senda de vida y luz;"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "mas Cristo le dijo: \"Si a Dios quieres ver,"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "tendrás que renacer\"."
      ]
    },
    "5": {
      "verse": "coro 1",
      "text": [
        "\"Tendrás que renacer,",
        "tendrás que renacer;"
      ]
    },
    "6": {
      "verse": "coro 1",
      "text": [
        "de cierto, de cierto te digo a ti:",
        "Tendrás que renacer\"."
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "Y tú, si en el cielo quisieres entrar"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "y con los benditos allí descansar;"
      ]
    },
    "9": {
      "verse": "2",
      "text": [
        "si vida eterna quisieres tener,",
        "tendrás que renacer."
      ]
    },
    "10": {
      "verse": "coro 2",
      "text": [
        "\"Tendrás que renacer,",
        "tendrás que renacer;"
      ]
    },
    "11": {
      "verse": "coro 2",
      "text": [
        "de cierto, de cierto te digo a ti:",
        "Tendrás que renacer\"."
      ]
    },
    "12": {
      "verse": "3",
      "text": [
        "Amigo, no debes jamás desechar"
      ]
    },
    "13": {
      "verse": "3",
      "text": [
        "palabras que Cristo dignose hablar;"
      ]
    },
    "14": {
      "verse": "3",
      "text": [
        "si tú no deseas el cielo perder,",
        "tendrás que renacer."
      ]
    },
    "15": {
      "verse": "coro 3",
      "text": [
        "\"Tendrás que renacer,",
        "tendrás que renacer;"
      ]
    },
    "16": {
      "verse": "coro 3",
      "text": [
        "de cierto, de cierto te digo a ti:",
        "Tendrás que renacer\"."
      ]
    },
    "17": {
      "verse": "4",
      "text": [
        "Hay quienes Jesús ha de resucitar,"
      ]
    },
    "18": {
      "verse": "4",
      "text": [
        "los cuales querrás aquel día encontrar;"
      ]
    },
    "19": {
      "verse": "4",
      "text": [
        "pues este mensaje hoy debes creer:",
        "Tendrás que renacer."
      ]
    },
    "20": {
      "verse": "coro 4",
      "text": [
        "\"Tendrás que renacer,",
        "tendrás que renacer;"
      ]
    },
    "21": {
      "verse": "coro 4",
      "text": [
        "de cierto, de cierto te digo a ti:",
        "Tendrás que renacer\"."
      ]
    }
  }
}