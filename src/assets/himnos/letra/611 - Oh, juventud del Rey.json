{
  "title": "Oh, juventud del Rey",
  "number": "611",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Oh, juventud del Rey,",
        "Jesús nos llama hoy."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Digámosle de corazón:",
        "mi vida yo te doy."
      ]
    },
    "3": {
      "verse": "2",
      "text": [
        "Cumplamos la misión,",
        "que Cristo nos dejó,",
        "de predicar su amor"
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "a quien Satán aprisionó."
      ]
    }
  }
}