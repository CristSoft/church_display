{
  "title": "Nunca desmayes",
  "number": "420",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Nunca desmayes, que en el afán",
        "Dios cuidará de ti;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "sus fuertes alas te cubrirán;",
        "Dios cuidará de ti."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Dios cuidará de ti;",
        "velando está su tierno amor;"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "sí, cuidará de ti, Dios cuidará de ti."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "En duras pruebas y en aflicción,",
        "Dios cuidará de ti;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "en tus conflictos y en tentación,",
        "Dios cuidará de ti."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Dios cuidará de ti;",
        "velando está su tierno amor;"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "sí, cuidará de ti, Dios cuidará de ti."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "De sus riquezas te suplirá;",
        "Dios cuidará de ti;"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "jamás sus bienes te negará;",
        "Dios cuidará de ti."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Dios cuidará de ti;",
        "velando está su tierno amor;"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "sí, cuidará de ti, Dios cuidará de ti."
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "Si vienen pruebas o cruel dolor,",
        "Dios cuidará de ti;"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "tus cargas pon sobre el Salvador;",
        "Dios cuidará de ti."
      ]
    },
    "15": {
      "verse": "coro",
      "text": [
        "Dios cuidará de ti;",
        "velando está su tierno amor;"
      ]
    },
    "16": {
      "verse": "coro",
      "text": [
        "sí, cuidará de ti, Dios cuidará de ti."
      ]
    }
  }
}