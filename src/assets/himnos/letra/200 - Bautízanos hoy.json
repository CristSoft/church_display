{
  "title": "Bautízanos hoy",
  "number": "200",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Bautízanos hoy",
        "con tu gran poder;",
        "tu amor nos renueve,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "acércate hoy."
      ]
    },
    "3": {
      "verse": "coro 1",
      "text": [
        "Humildes pedimos",
        "a ti, Salvador,",
        "que el Espíritu Santo"
      ]
    },
    "4": {
      "verse": "coro 1",
      "text": [
        "descienda aquí hoy."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Indignos del don,",
        "confiamos en ti.",
        "¡Oh, lava y limpia"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "hoy nuestra maldad!"
      ]
    },
    "7": {
      "verse": "coro 2",
      "text": [
        "Humildes pedimos",
        "a ti, Salvador,",
        "que el Espíritu Santo"
      ]
    },
    "8": {
      "verse": "coro 2",
      "text": [
        "descienda aquí hoy."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Paloma de amor,",
        "desciende aquí,",
        "otorga a tu pueblo"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "tu gran bendición."
      ]
    },
    "11": {
      "verse": "coro 3",
      "text": [
        "Humildes pedimos",
        "a ti, Salvador,",
        "que el Espíritu Santo"
      ]
    },
    "12": {
      "verse": "coro 3",
      "text": [
        "descienda aquí hoy."
      ]
    }
  }
}