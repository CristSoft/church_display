{
  "title": "Conversar con Jesucristo",
  "number": "389",
  "verses": "1",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Conversar con Jesucristo",
        "a través de la oración"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "llena el alma de alegría,",
        "trae paz al corazón;"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "y si tú también quisieras",
        "esta bendición gozar,"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "dondequiera te encontrares",
        "no te olvides de orar."
      ]
    }
  }
}