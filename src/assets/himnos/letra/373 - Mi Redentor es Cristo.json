{
  "title": "Mi Redentor es Cristo",
  "number": "373",
  "verses": "4",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Mi Redentor es Cristo,",
        "mi gozo y mi canción;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "me salva, y me consuela",
        "en horas de aflicción."
      ]
    },
    "3": {
      "verse": "coro 1",
      "text": [
        "¡Oh, Salvador bendito,",
        "mi canto elevo a ti;"
      ]
    },
    "4": {
      "verse": "coro 1",
      "text": [
        "cual tú no tengo amigo",
        "que me ame tanto aquí!"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "En todos mis afanes",
        "alivio y luz me das;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "aunque ande en negras sombras",
        "no dudaré jamás."
      ]
    },
    "7": {
      "verse": "coro 2",
      "text": [
        "¡Oh, Salvador bendito,",
        "mi canto elevo a ti;"
      ]
    },
    "8": {
      "verse": "coro 2",
      "text": [
        "cual tú no tengo amigo",
        "que me ame tanto aquí!"
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Jesús, en ti confío,",
        "amigo y guía fiel;"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "te quiero más que al mundo",
        "y cuanto se halla en él."
      ]
    },
    "11": {
      "verse": "coro 3",
      "text": [
        "¡Oh, Salvador bendito,",
        "mi canto elevo a ti;"
      ]
    },
    "12": {
      "verse": "coro 3",
      "text": [
        "cual tú no tengo amigo",
        "que me ame tanto aquí!"
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "Serás de mi alma el gozo",
        "aquí donde hay maldad;"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "también serás mi dicha",
        "allá en la eternidad."
      ]
    },
    "15": {
      "verse": "coro 4",
      "text": [
        "¡Oh, Salvador bendito,",
        "mi canto elevo a ti;"
      ]
    },
    "16": {
      "verse": "coro 4",
      "text": [
        "cual tú no tengo amigo",
        "que me ame tanto aquí!"
      ]
    }
  }
}