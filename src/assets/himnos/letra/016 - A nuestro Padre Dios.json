{
  "title": "A nuestro Padre Dios",
  "number": "16",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "A nuestro Padre Dios",
        "alcemos nuestra voz.",
        "¡Gloria a él!"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Tal fue su amor que dio",
        "al Hijo que murió,",
        "en quien confío yo."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "¡Gloria a él!"
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "A nuestro Salvador",
        "demos con fe loor.",
        "¡Gloria a él!"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Su sangre derramó;",
        "con ella me lavó;",
        "y el cielo me abrió."
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "¡Gloria a él!"
      ]
    },
    "7": {
      "verse": "3",
      "text": [
        "Espíritu de Dios,",
        "elevo a ti mi voz.",
        "¡Gloria a ti!"
      ]
    },
    "8": {
      "verse": "3",
      "text": [
        "Con celestial fulgor",
        "me muestras el amor",
        "de Cristo, mi Señor."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "¡Gloria a ti!"
      ]
    }
  }
}