{
  "title": "Hay un feliz Edén",
  "number": "330",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Hay un feliz Edén",
        "lejos de aquí,",
        "y gozará del bien"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "el justo allí.",
        "Cantemos con fervor:",
        "\"Digno eres, oh Señor,"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "de gloria y de honor; ¡loor a ti!\""
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "Marchad a aquel lugar,",
        "partid de aquí;"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "un bello y dulce hogar tendréis allí.",
        "¡Oh, cuán feliz seré"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "cuando a tu lado esté!",
        "Bendito viviré morando en ti."
      ]
    },
    "7": {
      "verse": "3",
      "text": [
        "Eterno resplandor",
        "fulgura allí.",
        "Eterno es el amor"
      ]
    },
    "8": {
      "verse": "3",
      "text": [
        "de Dios por mí.",
        "Corramos, pues, allá:",
        "bello aquel hogar será,"
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "por siempre habitará",
        "el santo allí."
      ]
    }
  }
}