{
  "title": "Cristo, nombre dulce",
  "number": "127",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Cristo, nombre dulce para mí;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Cristo, escuchas siempre mi oración."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "Oh, Cristo, tú me levantas si caí;"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "es muy dulce tu nombre, mi Jesús."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Cristo, alabarte es mi placer,"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "Cristo, primero y último eres tú."
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "oh, Cristo, por mí moriste en la cruz;"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "es muy dulce tu nombre, mi Jesús."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Cristo, eres Rey que volverá;",
        "Cristo, necesitamos tu amor."
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "Oh, Cristo, a ti cantamos con fervor;"
      ]
    },
    "11": {
      "verse": "3",
      "text": [
        "es muy dulce tu nombre, mi Jesús."
      ]
    }
  }
}