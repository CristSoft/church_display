{
  "title": "Mi fe contempla a ti",
  "number": "393",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Mi fe contempla a ti,",
        "Cordero celestial,",
        "mi Salvador."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Oye mi petición,",
        "quita mi transgresión,",
        "sea tu posesión"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "desde hoy, Señor."
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "Al débil corazón",
        "tu gracia pueda dar",
        "fe y valor."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Moriste tú por mí,",
        "sea mi amor a ti",
        "puro y ferviente así,"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "cual vivo ardor."
      ]
    },
    "7": {
      "verse": "3",
      "text": [
        "Por senda oscura voy,",
        "no veo en derredor;"
      ]
    },
    "8": {
      "verse": "3",
      "text": [
        "mas me guiarás. Enjuga mi llorar,",
        "mi noche haz brillar"
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "y así no pueda errar",
        "de ti jamás."
      ]
    }
  }
}