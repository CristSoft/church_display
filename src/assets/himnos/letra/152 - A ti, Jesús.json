{
  "title": "A ti, Jesús",
  "number": "152",
  "verses": "1",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "A ti, Jesús, alzaré mi voz,",
        "para adorarte, mi Salvador."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Acepta hoy, mi Rey y Señor,",
        "el humilde don de mi amor"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "en oración."
      ]
    }
  }
}