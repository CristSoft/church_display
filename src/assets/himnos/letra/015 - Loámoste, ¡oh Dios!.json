{
  "title": "Loámoste, ¡oh Dios!",
  "number": "015",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Loámoste, ¡Oh Dios!,",
        "con alegre canción,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "porque en Cristo tu Hijo",
        "nos diste perdón."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "¡Aleluya! Te alabamos.",
        "¡Cuán grande es tu amor!"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "¡Aleluya! Te adoramos,",
        "bendito Señor."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "A ti, oh Señor,",
        "que tu trono de luz",
        "has dejado por darnos"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "perdón en la cruz."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "¡Aleluya! Te alabamos.",
        "¡Cuán grande es tu amor!"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "¡Aleluya! Te adoramos,",
        "bendito Señor."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Te damos loor,",
        "santo Consolador,",
        "que nos llenas de gozo"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "y santo valor."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "¡Aleluya! Te alabamos.",
        "¡Cuán grande es tu amor!"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "¡Aleluya! Te adoramos,",
        "bendito Señor."
      ]
    }
  }
}