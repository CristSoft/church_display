{
  "title": "En tu nombre comenzamos",
  "number": "534",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "En tu nombre comenzamos",
        "esta escuela, ¡oh Señor!"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Con fervor te suplicamos",
        "seas nuestro Director."
      ]
    },
    "3": {
      "verse": "coro 1",
      "text": [
        "Cada sábado venimos",
        "a tu escuela, ¡oh Jesús!"
      ]
    },
    "4": {
      "verse": "coro 1",
      "text": [
        "Ven, Señor, a instruirnos",
        "en la ciencia de la cruz."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Esta escuela nos enseña",
        "tu Palabra a obedecer,"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "y tu Ley en nuestra vida",
        "ante el mundo a enaltecer."
      ]
    },
    "7": {
      "verse": "coro 2",
      "text": [
        "Cada sábado venimos",
        "a tu escuela, ¡oh Jesús!"
      ]
    },
    "8": {
      "verse": "coro 2",
      "text": [
        "Ven, Señor, a instruirnos",
        "en la ciencia de la cruz."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Ven, Señor, a enseñarnos",
        "tus preceptos a cumplir."
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "Ya reunidos, esperamos",
        "tu presencia aquí sentir."
      ]
    },
    "11": {
      "verse": "coro 3",
      "text": [
        "Cada sábado venimos",
        "a tu escuela, ¡oh Jesús!"
      ]
    },
