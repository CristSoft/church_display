{
  "title": "Himno #34 En momentos así",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "En momentos así",
        "yo canto canción,",
        "canción de amor a mi Cristo."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "En momentos así",
        "yo busco al Señor,",
        "elevo mi alma a Dios."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "Hoy te adoro, Señor,",
        "y te alabo, mi Dios;",
        "te amo, mi Jesús,"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "te amo, mi Señor."
      ]
    }
  }
}