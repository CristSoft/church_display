{
  "title": "Himno #470 Guíame, Dios",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Guíame, Dios,",
        "guía en tu justicia;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "tu senda allana frente a mí.",
        "Pues solo tú, Dios,"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "solo tú, Dios,",
        "me haces vivir confiado."
      ]
    }
  }
}