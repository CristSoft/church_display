{
  "title": "Amarte más",
  "number": "277",
  "verses": "4",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Amarte más, Jesús, anhelo hoy;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "oye mi humilde voz, pues tuyo soy."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Esta es mi oración:",
        "Quiero amarte más;",
        "amarte más,",
        "amarte más."
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "Del mundo el placer, antes busqué;"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "mas por amor a ti, todo dejé."
      ]
    },
    "6": {
      "verse": "coro",
      "text": [
        "Esta es mi oración:",
        "Quiero amarte más;",
        "amarte más,",
        "amarte más."
      ]
    },
    "7": {
      "verse": "3",
      "text": [
        "En medio del dolor refinador,"
      ]
    },
    "8": {
      "verse": "3",
      "text": [
        "más cerca iré de ti, mi Salvador."
      ]
    },
    "9": {
      "verse": "coro",
      "text": [
        "Esta es mi oración:",
        "Quiero amarte más;",
        "amarte más,",
        "amarte más."
      ]
    },
    "10": {
      "verse": "4",
      "text": [
        "Y mientras viva aquí te serviré;"
      ]
    },
    "11": {
      "verse": "4",
      "text": [
        "pues, por la eternidad, te amaré."
      ]
