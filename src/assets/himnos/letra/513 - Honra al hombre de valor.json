{
  "title": "Honra al hombre de valor",
  "number": "513",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Honra al hombre de valor,",
        "pronto a obedecer"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "el mandato del Señor,",
        "tal cual lo fue Daniel."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "A Daniel imita;",
        "dalo a conocer;",
        "muéstrate resuelto y firme,"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "aunque solo estés."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Muchos yacen sin valor,",
        "que pudieran ser"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "nobles héroes del Señor,",
        "tal cual lo fue Daniel."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "A Daniel imita;",
        "dalo a conocer;",
        "muéstrate resuelto y firme,"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "aunque solo estés."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Lucha en nombre del Señor",
        "sin desfallecer."
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "Sé en la lucha vencedor,",
        "tal cual lo fue Daniel."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "A Daniel imita;",
        "dalo a conocer;",
        "muéstrate resuelto y firme,"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "aunque solo estés."
      ]
    }
  }
}