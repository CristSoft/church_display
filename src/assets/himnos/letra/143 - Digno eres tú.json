{
  "title": "Digno eres tú",
  "number": "143",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Eres la Perla de valor;",
        "eres la Rosa de Sarón;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "eres mi Salvador.",
        "Eres la Roca de poder,"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "el Manantial para mi sed;",
        "contigo venceré."
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "Cristo, mi Señor, digno eres tú."
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "Cristo, mi Señor, digno eres tú."
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "Cuando tropiezo, cerca estás;",
        "tu fuerte brazo es mi sostén;"
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "eres mi Salvador.",
        "Cuando regreses triunfador"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "llévame salvo a tu mansión;",
        "contigo venceré."
      ]
    },
    "9": {
      "verse": "coro",
      "text": [
        "Cristo, mi Señor, digno eres tú."
      ]
    },
    "10": {
      "verse": "coro",
      "text": [
        "Cristo, mi Señor, digno eres tú."
      ]
    }
  }
}