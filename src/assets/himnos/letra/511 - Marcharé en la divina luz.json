{
  "title": "Marcharé en la divina luz",
  "number": "511",
  "verses": "4",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Marcharé en la divina luz,",
        "marcharé, siempre marcharé;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "en las huellas del buen Jesús,",
        "siempre marcharé."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Vestido blanco yo tendré,",
        "feliz allí podré gozar,"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "en el Edén sin par."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Marcharé con mi Guía fiel,",
        "marcharé, siempre marcharé;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "triunfaré por la fe en él,",
        "siempre marcharé."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Vestido blanco yo tendré,",
        "feliz allí podré gozar,"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "en el Edén sin par."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Marcharé almas a buscar,",
        "marcharé, siempre marcharé;"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "los perdidos a rescatar,",
        "siempre marcharé."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Vestido blanco yo tendré,",
        "feliz allí podré gozar,"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "en el Edén sin par."
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "Por Jesús, más que vencedor",
        "marcharé, siempre marcharé;"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "al hogar de mi Salvador,",
        "siempre marcharé."
      ]
    },
    "15": {
      "verse": "coro",
      "text": [
        "Vestido blanco yo tendré,",
        "feliz allí podré gozar,"
      ]
    },
    "16": {
