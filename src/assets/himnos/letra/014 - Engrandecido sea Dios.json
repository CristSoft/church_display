{
  "title": "Himno #14 Engrandecido sea Dios",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Engrandecido sea Dios",
        "en esta ocasión."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Alegres, juntos a una voz",
        "dad gloria, gloria, gloria"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "al Dios eternal."
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "Durante el día que pasó,",
        "la mano del Señor"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "de muchos males nos salvó:",
        "dad gloria, gloria, gloria"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "al Dios eternal."
      ]
    },
    "7": {
      "verse": "3",
      "text": [
        "Él hasta aquí nos ayudó,",
        "y siempre proveerá."
      ]
    },
    "8": {
      "verse": "3",
      "text": [
        "Con gratitud, placer y amor",
        "dad gloria, gloria, gloria"
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "al Dios eternal."
      ]
    },
    "10": {
      "verse": "4",
      "text": [
        "A otras almas salva, ¡oh Dios!",
        "Despiértalas, Señor;"
      ]
    },
    "11": {
      "verse": "4",
      "text": [
        "escucha nuestra petición,",
        "y salva, salva, salva,"
      ]
    },
    "12": {
      "verse": "4",
      "text": [
        "salva al pecador."
      ]
    }
  }
}