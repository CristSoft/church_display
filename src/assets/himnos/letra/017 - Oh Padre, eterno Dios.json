{
  "title": "Oh Padre, eterno Dios",
  "number": "17",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Oh Padre, eterno Dios,",
        "alzamos nuestra voz",
        "en gratitud"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "por lo que tú nos das",
        "con sin igual amor,",
        "y hallamos dulce paz"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "en ti, Señor."
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "Bendito Salvador,",
        "te damos con amor",
        "el corazón;"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "acepta, oh Señor,",
        "lo que en tu altar",
        "venimos a ofrendar"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "cual vivo don."
      ]
    },
    "7": {
      "verse": "3",
      "text": [
        "Espíritu de Dios,",
        "escucha nuestra voz,",
        "y en tu bondad"
      ]
    },
    "8": {
      "verse": "3",
      "text": [
        "derrama en nuestro ser",
        "divina claridad,",
        "copiosa bendición"
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "y santidad."
      ]
    }
  }
}