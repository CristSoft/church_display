{
  "title": "Soy yo, Señor",
  "number": "390",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Soy yo, soy yo, soy yo, Señor,",
        "el que necesita orar."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Soy yo, soy yo, soy yo, Señor,",
        "el que necesita orar."
      ]
    },
    "3": {
      "verse": "2",
      "text": [
        "No es mi madre, ni mi padre,",
        "sino yo, Señor,"
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "el que necesita orar.",
        "No es mi hermano, ni mi hermana,"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "sino yo, Señor",
        "el que necesita orar."
      ]
    }
  }
