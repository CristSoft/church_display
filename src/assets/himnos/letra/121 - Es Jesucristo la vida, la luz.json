{
  "title": "Es Jesucristo la vida, la luz",
  "number": "121",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Es Jesucristo la vida, la luz;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "trae del cielo la eterna verdad;"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "mártir divino que muere en la cruz",
        "por darnos libertad."
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "Cristo es Pastor, Enviado,",
        "divino Emanuel."
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "Él me conduce por sendas de paz",
        "como a su oveja fiel."
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "Quita del alma la negra maldad,"
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "limpia benigno el infiel corazón;"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "es su carácter de suma bondad,",
        "la misma compasión."
      ]
    },
    "9": {
      "verse": "coro",
      "text": [
        "Cristo es Pastor, Enviado,",
        "divino Emanuel."
      ]
    },
    "10": {
      "verse": "coro",
      "text": [
        "Él me conduce por sendas de paz",
        "como a su oveja fiel."
      ]
    },
    "11": {
      "verse": "3",
      "text": [
        "Fuente preciosa de gracia y salud,"
      ]
    },
    "12": {
      "verse": "3",
      "text": [
        "agua que limpia de toda maldad."
      ]
    },
    "13": {
      "verse": "3",
      "text": [
        "Quiere llenarnos de su plenitud",
        "y de su santidad."
      ]
    },
    "14": {
      "verse": "coro",
      "text": [
        "Cristo es Pastor, Enviado,",
        "divino Emanuel."
      ]
    },
    "15": {
      "verse": "coro",
      "text": [
        "Él me conduce por sendas de paz",
        "como a su oveja fiel."
      ]
    }
  }
}