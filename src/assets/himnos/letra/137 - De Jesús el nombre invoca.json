{
  "title": "De Jesús el nombre invoca",
  "number": "137",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "De Jesús el nombre invoca,",
        "heredero del dolor,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "dulce hará tu amarga copa",
        "con el néctar de su amor."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Suave luz, manantial",
        "de esperanza, fe y amor;"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "sumo bien celestial",
        "es Jesús, el Salvador."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "De Jesús el nombre estima,",
        "que te sirva de broquel;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "alma débil, combatida,",
        "hallarás asilo en él."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Suave luz, manantial",
        "de esperanza, fe y amor;"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "sumo bien celestial",
        "es Jesús, el Salvador."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "De Jesús el nombre ensalza,",
        "cuyo sin igual poder"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "del sepulcro nos levanta,",
        "renovando nuestro ser."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Suave luz, manantial",
        "de esperanza, fe y amor;"
      ]
