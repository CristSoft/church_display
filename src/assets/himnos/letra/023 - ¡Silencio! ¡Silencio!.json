{
  "title": "¡Silencio! ¡Silencio!",
  "number": "23",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "¡Silencio! ¡Silencio!",
        "en este lugar;",
        "¡silencio! silencio"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "habéis de guardar."
      ]
    },
    "3": {
      "verse": "coro 1",
      "text": [
        "Quedad en silencio",
        "en este lugar;",
        "silencio, silencio"
      ]
    },
    "4": {
      "verse": "coro 1",
      "text": [
        "guardad al orar."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "¡Silencio! ¡Silencio!",
        "Es tiempo de orar,"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "la gracia divina podréis disfrutar."
      ]
    },
    "7": {
      "verse": "coro 2",
      "text": [
        "Quedad en silencio",
        "en este lugar;",
        "silencio, silencio"
      ]
    },
    "8": {
      "verse": "coro 2",
      "text": [
        "guardad al orar."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "¡Silencio! ¡Silencio!",
        "su amor recordad."
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "A Dios, pues, postrados,",
        "honor tributad."
      ]
    },
    "11": {
      "verse": "coro 3",
      "text": [
        "Quedad en silencio",
        "en este lugar;",
        "silencio, silencio"
      ]
    },
    "12": {
      "verse": "coro 3",
      "text": [
        "guardad al orar."
      ]
    }
  }
}