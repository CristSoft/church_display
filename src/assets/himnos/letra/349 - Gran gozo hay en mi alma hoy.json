{
  "title": "Gran gozo hay en mi alma hoy",
  "number": "349",
  "verses": "4",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Gran gozo hay en mi alma hoy:",
        "Jesús conmigo está;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "contento con su amor estoy,",
        "su dulce paz me da."
      ]
    },
    "3": {
      "verse": "coro 1",
      "text": [
        "Brilla el sol de Cristo en mi alma;",
        "cada día voy feliz así."
      ]
    },
    "4": {
      "verse": "coro 1",
      "text": [
        "Su faz sonriente al contemplar,",
        "¡cuánto gozo siento en mí!"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "En mi alma hay melodías hoy,",
        "canciones a mi Rey."
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "Feliz y libre en Cristo soy",
        "y salvo por la fe."
      ]
    },
    "7": {
      "verse": "coro 2",
      "text": [
        "Brilla el sol de Cristo en mi alma;",
        "cada día voy feliz así."
      ]
    },
    "8": {
      "verse": "coro 2",
      "text": [
        "Su faz sonriente al contemplar,",
        "¡cuánto gozo siento en mí!"
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Paz plena tengo en mi alma hoy,",
        "pues Cristo me salvó."
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "Mis hierros rotos quedan ya:",
        "Jesús me libertó."
      ]
    },
    "11": {
      "verse": "coro 3",
      "text": [
        "Brilla el sol de Cristo en mi alma;",
        "cada día voy feliz así."
      ]
    },
    "12": {
      "verse": "coro 3",
      "text": [
        "Su faz sonriente al contemplar,",
        "¡cuánto gozo siento en mí!"
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "En mi alma hoy reina gratitud",
        "y loores a Jesús."
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "En su presencia hay virtud,",
        "hay gozo en su luz."
      ]
    },
