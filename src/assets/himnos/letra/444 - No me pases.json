{
  "title": "No me pases",
  "number": "444",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "No me pases, no me olvides,",
        "tierno Salvador;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "muchos gozan tus mercedes,",
        "oye mi clamor."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Cristo, Cristo, oye tú mi voz;"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "Salvador, tu gracia dame, oye mi clamor."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Ante el trono de tu gracia",
        "hallo dulce paz;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "nada aquí mi alma sacia,",
        "tú eres mi solaz."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Cristo, Cristo, oye tú mi voz;"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "Salvador, tu gracia dame, oye mi clamor."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Solo fío en tus bondades,",
        "guíame en tu luz,"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "y mi alma no deseches,",
        "sálvame, Jesús."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Cristo, Cristo, oye tú mi voz;"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "Salvador, tu gracia dame, oye mi clamor."
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "Fuente viva de consuelo",
        "eres para mí;"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "mi alma pone en ti su anhelo,",
        "solamente en ti."
      ]
    },
    "15": {
      "verse": "coro",
      "text": [
        "Cristo, Cristo, oye tú mi voz;"
      ]
    },
    "16": {
      "verse": "coro",
      "text": [
        "Salvador, tu gracia dame, oye mi clamor."
      ]
    }
  }
}