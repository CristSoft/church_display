{
  "title": "Sus manos somos",
  "number": "496",
  "verses": "2",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Sus manos somos,",
        "que ayudan en el mundo.",
        "Somos sus pies,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "que van donde él los guía."
      ]
    },
    "3": {
      "verse": "coro 1",
      "text": [
        "Somos su amor,",
        "lumbre en las tinieblas.",
        "Somos su luz"
      ]
    },
    "4": {
      "verse": "coro 1",
      "text": [
        "en la oscuridad."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Sus ojos somos,",
        "que ven a los sufrientes.",
        "Somos su voz,"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "que anuncia su regreso."
      ]
    },
    "7": {
      "verse": "coro 2",
      "text": [
        "Somos su amor,",
        "lumbre en las tinieblas.",
        "Somos su luz"
      ]
    },
    "8": {
      "verse": "coro 2",
      "text": [
        "en la oscuridad."
      ]
    }
  }
}