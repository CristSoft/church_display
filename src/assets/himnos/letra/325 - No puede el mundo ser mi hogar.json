{
  "title": "No puede el mundo ser mi hogar",
  "number": "325",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "No puede el mundo ser mi hogar,",
        "sus bienes han de perecer;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "anhelo pronto a Sion llegar",
        "y a mi Maestro ver."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Iré al hogar,",
        "a mi eterno y dulce hogar,",
        "mi hogar;"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "sí, iré al hogar que Jesús fue a preparar."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "La muerte allí no existirá;",
        "tampoco la separación;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "tristeza nunca más habrá",
        "en la anhelada Sion."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Iré al hogar,",
        "a mi eterno y dulce hogar,",
        "mi hogar;"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "sí, iré al hogar que Jesús fue a preparar."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Deseo pronto arribar",
        "al puerto de la eterna paz."
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "Con Cristo quiero allí reinar",
        "dejando el mundo atrás."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Iré al hogar,",
        "a mi eterno y dulce hogar,",
        "mi hogar;"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "sí, iré al hogar que Jesús fue a preparar."
      ]
    }
  }
}