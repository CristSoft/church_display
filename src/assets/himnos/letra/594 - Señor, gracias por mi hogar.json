{
  "title": "Señor, gracias por mi hogar",
  "number": "594",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Horas alegres hay",
        "en la niñez feliz,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "mientras mamá vigila fiel.",
        "Señor gracias, por mi hogar"
      ]
    },
    "3": {
      "verse": "2",
      "text": [
        "Horas inquietas hay",
        "cuando hay que decidir:"
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "sabios consejos recibir,",
        "Señor, gracias por mi hogar."
      ]
    },
    "5": {
      "verse": "3",
      "text": [
        "Horas serenas hay",
        "para reflexionar,",
        "y agradecidos hoy decir:"
      ]
    },
    "6": {
      "verse": "3",
      "text": [
        "Señor, gracias por mi hogar."
      ]
    }
  }
}