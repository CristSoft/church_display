{
  "title": "Despide hoy tu grey",
  "number": "39",
  "verses": "2",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Despide hoy tu grey",
        "en paz y bendición,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "y las palabras de tu Ley",
        "conserve el corazón."
      ]
    },
    "3": {
      "verse": "2",
      "text": [
        "Enséñanos, Señor,",
        "tu Ley a meditar,",
        "vivir unidos en amor,"
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "y en él por siempre andar."
      ]
    }
  }
}