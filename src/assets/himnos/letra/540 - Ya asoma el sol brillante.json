{
  "title": "Ya asoma el sol brillante",
  "number": "540",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Ya asoma el sol brillante,",
        "vertiendo luz, calor;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "natura alegre canta:",
        "es día del Señor."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Hoy, sábado, reunidos",
        "en culto a ti, Señor,"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "tus hijos redimidos",
        "te rinden su loor."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Perfume de las flores",
        "se eleva hacia Dios."
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "Los pajarillos trinan",
        "con melodiosa voz."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Hoy, sábado, reunidos",
        "en culto a ti, Señor,"
      ]
    },
    "8": {
