{
  "title": "Hoy me llama el mundo en vano",
  "number": "271",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Hoy me llama el mundo en vano,",
        "quiero ser cual Cristo;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "ya no sirvo a lo mundano,",
        "quiero ser cual Cristo."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "¡Ser como él de corazón!,",
        "es mi sola aspiración;"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "en cualquiera condición",
        "quiero ser cual Cristo."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Mis cadenas Cristo ha roto,",
        "quiero ser cual Cristo;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "su servicio haré devoto,",
        "quiero ser cual Cristo."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "¡Ser como él de corazón!,",
        "es mi sola aspiración;"
      ]
    },
    "8": {
