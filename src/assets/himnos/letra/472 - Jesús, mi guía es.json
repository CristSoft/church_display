{
  "title": "Jesús, mi guía es",
  "number": "472",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Jesús, mi guía es,",
        "mi amigo y protector,",
        "refugio de todo mal;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "y cuando temor me da,",
        "yo confío en él.",
        "Yo confío en él;"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "aunque débil soy",
        "me da su poder y su amor."
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "Jesús, mi Redentor,",
        "me guardas del temor,",
        "y llenas mi corazón"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "con cantos de salvación;",
        "confiaré en ti.",
        "Yo confío en ti;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "aunque débil soy",
        "me das tu poder y tu amor.",
        "Yo confío en ti."
      ]
    }
  }
}