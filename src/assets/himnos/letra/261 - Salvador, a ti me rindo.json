{
  "title": "Salvador, a ti me rindo",
  "number": "261",
  "verses": "4",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Salvador, a ti me rindo,",
        "obedezco sólo a ti."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Mi Guiador, mi Fortaleza,",
        "todo encuentro, oh Cristo, en ti."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Yo me rindo a ti,",
        "yo me rindo a ti;",
        "mis flaquezas,",
        "mis pecados,"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "todo rindo a ti."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Te confiesa su delito",
        "mi contrito corazón."
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "Oye, Cristo, mi plegaria;",
        "quiero en ti tener perdón."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Yo me rindo a ti,",
        "yo me rindo a ti;",
        "mis flaquezas,",
        "mis pecados,"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "todo rindo a ti."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "A tus pies, Señor, entrego",
        "bienes, goces y placer."
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "Que tu Espíritu me llene,",
        "y de ti sienta poder."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Yo me rindo a ti,",
        "yo me rindo a ti;",
        "mis flaquezas,",
        "mis pecados,"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "todo rindo a ti."
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "¡Oh, qué gozo encuentro en Cristo!",
        "¡Cuánta paz a mi alma da!"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "A su causa me consagro,",
        "y su amor mi amor será."
      ]
    },
    "15": {
      "verse": "coro",
      "text": [
        "Yo me rindo a ti,",
        "yo me rindo a ti;",
        "mis flaquezas,",
        "mis pecados,"
      ]
    },
    "16": {
      "verse": "coro",
      "text": [
        "todo rindo a ti."
      ]
    }
  }
