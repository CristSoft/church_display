{
  "title": "Mientras Jesús te llama",
  "number": "214",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Mientras Jesús te llama,",
        "ven, pecador.",
        "Mientras por ti oramos,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "ven, pecador.",
        "Hoy es el día propicio.",
        "Ven, pecador."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "Hoy puedes conocerle.",
        "Ven, pecador."
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "¿Andas de mal cargado?",
        "Ven, pecador."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Hay en Jesús alivio. Ven, pecador.",
        "No quiere engañarte."
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "Ven, pecador.",
        "Hoy quiere rescatarte.",
        "Ven, pecador."
      ]
    },
    "7": {
      "verse": "3",
      "text": [
        "Oye sus tiernos ruegos.",
        "Ven, pecador."
      ]
    },
    "8": {
      "verse": "3",
      "text": [
        "Su bendición recibe. Ven, pecador.",
        "Mientras Jesús te llama,"
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "ven, pecador.",
        "Mientras por ti oramos,",
        "ven, pecador."
      ]
    }
  }
}