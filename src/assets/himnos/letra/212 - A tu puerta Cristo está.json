{
  "title": "A tu puerta Cristo está",
  "number": "212",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "A tu puerta Cristo está.",
        "Ábrele.",
        "Si le abres entrará."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Ábrele. Tu pecado quitará,",
        "luz y paz derramará,"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "su perdón te otorgará.",
        "Ábrele."
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "Ábrele, oh pecador.",
        "Ábrele.",
        "Al amante Salvador",
        "ábrele."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Hoy te ofrece salvación,",
        "del pecado el perdón;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "saciará tu corazón.",
        "Ábrele."
      ]
    },
    "7": {
      "verse": "3",
      "text": [
        "No le hagas esperar.",
        "Ábrele.",
        "No le obligues a marchar."
      ]
    },
    "8": {
      "verse": "3",
      "text": [
        "Ábrele. ¡Qué dolor después tendrás,",
        "cuando en vano clamarás"
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "y perdido te hallarás!",
        "Ábrele."
      ]
    }
  }
}