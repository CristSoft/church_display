{
  "title": "Tocad trompeta ya",
  "number": "575",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Tocad trompeta ya,",
        "alegres en Sion;",
        "al mundo proclamad"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "la eterna redención."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Este es el año de bondad,",
        "volved a vuestra libertad,"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "volved a vuestra libertad."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "A Cristo predicad;",
        "decid que ya murió",
        "y con su potestad"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "la muerte destruyó."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Este es el año de bondad,",
        "volved a vuestra libertad,"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "volved a vuestra libertad."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Vosotros que el favor",
        "del Cielo despreciáis,"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "su gracia gozaréis",
        "si a Cristo pronto vais."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Este es el año de bondad,",
        "volved a vuestra libertad,"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "volved a vuestra libertad."
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "Llamadles con amor;",
        "id, ofrecedles paz.",
        "Es tarde, apresurad;"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "que vuelvan a su faz."
      ]
    },
    "15": {
      "verse": "coro",
      "text": [
        "Este es el año de bondad,",
        "volved a vuestra libertad,"
      ]
    },
    "16": {
      "verse": "coro",
      "text": [
        "volved a vuestra libertad."
      ]
    }
  }
}