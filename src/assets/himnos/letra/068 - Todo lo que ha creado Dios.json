{
  "title": "Todo lo que ha creado Dios",
  "number": "068",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Todo lo que ha creado Dios"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "alce su voz para cantar: Aleluya, aleluya."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "Dorado y brillante sol,",
        "y luna de plateada luz."
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "Alabadle, alabadle;",
        "aleluya, aleluya, aleluya."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Ligera brisa o vendaval,"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "nube que cruza el cielo azul: Alabadle, aleluya."
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "Rosado y bello amanecer,",
        "tranquilo y calmo anochecer."
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "Alabadle, alabadle;",
        "aleluya, aleluya, aleluya."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Rugiente y tempestuoso mar,"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "arroyo alegre al correr: Alabadle, aleluya."
      ]
    },
    "11": {
      "verse": "3",
      "text": [
        "Oh, fuego ardiente, abrazador,",
        "que da calor a nuestro hogar."
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "Alabadle, alabadle;",
        "aleluya, aleluya, aleluya."
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "Todas las cosas, adorad"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "con humildad al Creador, alabadle, aleluya."
      ]
    },
    "15": {
      "verse": "4",
      "text": [
        "Todo lo que ha creado Dios",
        "alabe al soberano Rey."
      ]
    },
    "16": {
      "verse": "coro",
      "text": [
        "Alabadle, alabadle;",
        "aleluya, aleluya, aleluya."
      ]
    }
  }
}