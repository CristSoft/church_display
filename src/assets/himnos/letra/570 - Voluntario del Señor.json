{
  "title": "Voluntario del Señor",
  "number": "570",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Voluntario del Señor,",
        "listo a servir,",
        "a llevar el evangelio"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "do me toque ir."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Voluntad, voluntad,",
        "Cristo me pidió.",
        "Voluntad, voluntad,"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "eso ofrezco yo;",
        "para Cristo quiero ser",
        "fiel trabajador,"
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "misionero voluntario",
        "para el Salvador."
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "En su viña quiero obrar",
        "y ocupado estar,",
        "mientras intercede"
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "Cristo presto a salvar."
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "Voluntad, voluntad,",
        "Cristo me pidió.",
        "Voluntad, voluntad,"
      ]
    },
    "9": {
