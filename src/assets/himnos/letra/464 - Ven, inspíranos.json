{
  "title": "Ven, inspíranos",
  "number": "464",
  "verses": "2",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Ven, inspíranos; ven, enséñanos;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Señor, por medio de tu Espíritu."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "Ven, anímanos; ven, transfórmanos;"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "Señor, con tu poder y gloria."
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "Señor, queremos dar loor",
        "en gratitud a ti."
      ]
    },
    "6": {
      "verse": "coro",
      "text": [
        "Acepta nuestro corazón;",
        "Ven y transfórmalo hoy."
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "Ven, ayúdanos; ven, consuélanos;"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "Señor, concédenos tu dulce paz.",
        "Danos tu poder para obedecer"
      ]
    },
    "9": {
      "verse": "2",
      "text": [
        "y amarte por tu sacrificio."
      ]
    },
    "10": {
      "verse": "coro",
      "text": [
        "Señor, queremos dar loor",
        "en gratitud a ti."
      ]
    },
