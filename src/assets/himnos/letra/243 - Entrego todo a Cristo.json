{
  "title": "Entrego todo a Cristo",
  "number": "243",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Todo mi vil pecado",
        "dejo ante mi Jesús;",
        "él lleva toda carga,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "liberación me da,",
        "liberación me da."
      ]
    },
    "3": {
      "verse": "2",
      "text": [
        "Mis culpas traigo a Cristo,",
        "vengo a lavar mi mal;"
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "en su preciosa sangre,",
        "cual nieve quedará,",
        "cual nieve quedará."
      ]
    },
    "5": {
      "verse": "3",
      "text": [
        "Le entrego mi miseria,",
        "su plenitud me da;"
      ]
    },
    "6": {
      "verse": "3",
      "text": [
        "él sana mi alma enferma,",
        "y redención me da,",
        "y redención me da."
      ]
    },
    "7": {
      "verse": "4",
      "text": [
        "Yo quiero ser cual Cristo,",
        "con humildad vivir;"
      ]
    },
    "8": {
      "verse": "4",
      "text": [
        "yo anhelo estar con Cristo,",
        "siempre vivir con él."
      ]
    },
    "9": {
      "verse": "4",
      "text": [
        "siempre vivir con él."
      ]
    }
  }
}