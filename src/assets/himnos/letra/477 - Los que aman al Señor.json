{
  "title": "Los que aman al Señor",
  "number": "477",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Los que aman al Señor",
        "eleven su canción,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "que en dulces notas de loor,",
        "que en dulces notas de loor,"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "ascienda a su mansión,",
        "ascienda a su mansión."
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "A Sion caminamos,",
        "nuestra ciudad tan gloriosa;"
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "marchando todos cantamos",
        "de Dios y la bella mansión."
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "Unida está, oh Dios,",
        "tu fiel y amada grey;"
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "y cantan todos a una voz,",
        "y cantan todos a una voz,"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "los hijos del gran Rey,",
        "los hijos del gran Rey."
      ]
    },
    "9": {
      "verse": "coro",
      "text": [
        "A Sion caminamos,",
        "nuestra ciudad tan gloriosa;"
      ]
    },
    "10": {
      "verse": "coro",
      "text": [
        "marchando todos cantamos",
        "de Dios y la bella mansión."
      ]
    },
    "11": {
      "verse": "3",
      "text": [
        "Cantemos con fervor,",
        "dejemos el pesar,"
      ]
    },
    "12": {
      "verse": "3",
      "text": [
        "marchemos libres de temor,",
        "marchemos libres de temor,"
      ]
    },
    "13": {
      "verse": "3",
      "text": [
        "al más feliz hogar,",
        "al más feliz hogar."
      ]
    },
    "14": {
