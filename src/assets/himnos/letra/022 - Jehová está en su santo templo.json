{
  "title": "Jehová está en su santo templo",
  "number": "022",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Jehová está en su santo templo,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Jehová está en su santo templo;",
        "seamos reverentes,"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "seamos reverentes",
        "ante el Señor.",
        "¡Silencio!, ¡silencio!,"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "ante el Señor.",
        "Amén."
      ]
    }
  }
}