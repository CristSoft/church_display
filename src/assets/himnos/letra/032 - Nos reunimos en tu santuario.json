{
  "title": "Nos reunimos en tu santuario",
  "number": "32",
  "verses": "1",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Nos reunimos en tu santuario,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "donde santos ángeles te rinden su loor."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "Vamos a adorar tu gran nombre;"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "en la santidad de tu presencia te alabo, Señor."
      ]
    }
  }
}