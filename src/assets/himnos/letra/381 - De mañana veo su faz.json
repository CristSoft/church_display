{
  "title": "De mañana veo su faz",
  "number": "381",
  "verses": "1",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "De mañana, veo su faz;",
        "en la tarde, me da solaz"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "siendo oscuro, su voz da luz;",
        "por doquiera yo voy con Jesús."
      ]
    }
  }
}