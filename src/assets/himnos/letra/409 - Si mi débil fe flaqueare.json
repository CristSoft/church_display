{
  "title": "Si mi débil fe flaqueare",
  "number": "409",
  "verses": "4",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Si mi débil fe flaqueare,",
        "él me sostendrá."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Si el mal me amenazare,",
        "él me sostendrá."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Él me sostendrá,",
        "él me sostendrá.",
        "Porque me ama el Salvador,"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "él me sostendrá."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Nunca yo podré afirmarme",
        "con tan débil fe;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "mas él puede dirigirme,",
        "y me sostendrá."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Él me sostendrá,",
        "él me sostendrá.",
        "Porque me ama el Salvador,"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "él me sostendrá."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Son su gozo y complacencia",
        "cuantos él salvó,"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "y al salvarme su clemencia,",
        "él me sostendrá."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Él me sostendrá,",
        "él me sostendrá.",
        "Porque me ama el Salvador,"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "él me sostendrá."
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "Él no quiso ver perdida",
        "mi alma en la maldad;"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "dio su sangre por mi vida,",
        "y él me sostendrá."
      ]
    },
    "15": {
      "verse": "coro",
      "text": [
        "Él me sostendrá,",
        "él me sostendrá.",
        "Porque me ama el Salvador,"
      ]
    },
    "16": {
      "verse": "coro",
      "text": [
        "él me sostendrá."
      ]
    }
  }
