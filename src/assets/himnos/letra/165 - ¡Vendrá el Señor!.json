      "text": [
        "¡Aleluya! ¡Aleluya!;",
        "En las nubes vendrá"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "con sus huestes gloriosas;",
        "sí, muy pronto vendrá."
      ]
    },
    "13": {
      "verse": "3",
      "text": [
        "Velad y orad",
        "con la vista alzada;",
        "salid y luchad"
      ]
    },
    "14": {
      "verse": "3",
      "text": [
        "con la santa espada;",
        "id a trabajar",
        "con la fe reanimada,"
      ]
    },
    "15": {
      "verse": "3",
      "text": [
        "pues muy pronto vendrá."
      ]
