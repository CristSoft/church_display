{
  "title": "¡Vendrá el Señor!",
  "number": "165",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "¡Vendrá el Señor!",
        "Nadie sabe la hora;",
        "del día anhelado"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "se ve la aurora.",
        "¡Oh, siervos de Dios!,",
        "anunciad sin demora"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "que muy pronto vendrá."
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "¡Él vendrá!",
        "¡Esperad y velad,",
        "pues él vendrá!"
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "¡Aleluya! ¡Aleluya!;",
        "En las nubes vendrá"
      ]
    },
    "6": {
      "verse": "coro",
      "text": [
        "con sus huestes gloriosas;",
        "sí, muy pronto vendrá."
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "En cielo y tierra",
        "se anuncia el portento",
        "de Cristo en su"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "glorioso advenimiento.",
        "¡Oh, pueblo de Dios!,"
      ]
    },
    "9": {
      "verse": "2",
      "text": [
        "es solemne el momento,",
        "pues muy pronto vendrá."
      ]
    },
    "10": {
      "verse": "coro",
      "text": [
        "¡Él vendrá!",
        "¡Esperad y velad,",
        "pues él vendrá!"
      ]
    },
    "11": {
