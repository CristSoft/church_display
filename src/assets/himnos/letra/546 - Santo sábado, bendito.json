{
  "title": "Santo sábado, bendito",
  "number": "546",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Santo sábado, bendito,",
        "joya de la creación,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "que trasciende todo cambio,",
        "es eterna bendición."
      ]
    },
    "3": {
      "verse": "coro 1",
      "text": [
        "Santo sábado de Dios,",
        "para el hombre lo creó."
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "El pecado y la tristeza",
        "pronto no existirán."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Pero el sábado es eterno,",
        "y por siempre durará."
      ]
    },
    "6": {
      "verse": "coro 2",
      "text": [
        "Santo sábado de Dios,",
        "para el hombre lo creó."
      ]
    },
    "7": {
      "verse": "3",
      "text": [
        "El reposo verdadero",
        "hoy buscamos, Dios, en ti."
      ]
    },
    "8": {
      "verse": "3",
      "text": [
        "Que la paz del día santo,",
        "sea un remanso aquí."
      ]
    },
    "9": {
      "verse": "coro 3",
      "text": [
        "Santo sábado de Dios,",
        "para el hombre lo creó."
      ]
    }
  }
}