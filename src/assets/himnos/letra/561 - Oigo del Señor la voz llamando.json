{
  "title": "Oigo del Señor la voz llamando",
  "number": "561",
  "verses": "4",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Oigo del Señor la voz llamando:"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "\"¿Quién irá este día a trabajar?"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "¿Quién me traerá a los perdidos?"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "¿Quién la senda angosta mostrará?\""
      ]
    },
    "5": {
      "verse": "coro 1",
      "text": [
        "Habla, oh Dios, háblame;",
        "habla, y pronto te contestaré;"
      ]
    },
    "6": {
      "verse": "coro 1",
      "text": [
        "habla, oh Dios, háblame,",
        "habla, y yo respondo: \"Heme aquí\"."
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "Cuando el mismo Dios tocó al profeta,"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "dándole un nuevo corazón,",
        "y este oyó la voz que le llamaba:"
      ]
    },
    "9": {
      "verse": "2",
      "text": [
        "\"Heme aquí\", él pronto contestó."
      ]
    },
    "10": {
      "verse": "coro 2",
      "text": [
        "Habla, oh Dios, háblame;",
        "habla, y pronto te contestaré;"
      ]
    },
    "11": {
      "verse": "coro 2",
      "text": [
        "habla, oh Dios, háblame,",
        "habla, y yo respondo: \"Heme aquí\"."
      ]
    },
    "12": {
      "verse": "3",
      "text": [
        "Muchos miles y millones mueren,",
        "en la más completa oscuridad;"
      ]
    },
    "13": {
      "verse": "3",
      "text": [
        "anda pronto tú a rescatarlos;"
      ]
    },
    "14": {
      "verse": "3",
      "text": [
        "di: \"Maestro, voy con voluntad\"."
      ]
    },
    "15": {
      "verse": "coro 3",
      "text": [
        "Habla, oh Dios, háblame;",
        "habla, y pronto te contestaré;"
      ]
    },
    "16": {
      "verse": "coro 3",
      "text": [
        "habla, oh Dios, háblame,",
        "habla, y yo respondo: \"Heme aquí\"."
      ]
    },
    "17": {
      "verse": "4",
      "text": [
        "Pronto el Redentor vendrá en gloria,"
      ]
    },
    "18": {
      "verse": "4",
      "text": [
        "las trompetas lo anunciarán;",
        "y entonces se oirá al Maestro"
      ]
    },
    "19": {
      "verse": "4",
      "text": [
        "que dirá: \"Bien hecho, siervo fiel\"."
      ]
    },
    "20": {
      "verse": "coro 4",
      "text": [
        "Habla, oh Dios, háblame;",
        "habla, y pronto te contestaré;"
      ]
    },
