{
  "title": "Con gratitud, llegamos",
  "number": "525",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Con gratitud, llegamos",
        "a tu altar, Señor;",
        "gozosos te adoramos,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "cantamos con fervor."
      ]
    },
    "3": {
      "verse": "2",
      "text": [
        "Queremos devolverte",
        "el diezmo sin tardar;"
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "por eso hoy, fielmente,",
        "lo vamos a entregar."
      ]
    },
    "5": {
      "verse": "3",
      "text": [
        "Con gozo, nuestra ofrenda",
        "te damos, oh Señor;"
      ]
    },
    "6": {
      "verse": "3",
      "text": [
        "acéptala cual prenda",
        "de todo nuestro amor."
      ]
    }
  }
}