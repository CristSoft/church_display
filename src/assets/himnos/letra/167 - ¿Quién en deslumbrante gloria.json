{
  "title": "¿Quién en deslumbrante gloria?",
  "number": "167",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "¿Quién en deslumbrante gloria",
        "con sus huestes está?"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "¿Quién con célica armadura",
        "delante de ellos va?"
      ]
    },
    "3": {
      "verse": "coro 1",
      "text": [
        "¡Ve adelante, Señor, supremo Rey!"
      ]
    },
    "4": {
      "verse": "coro 1",
      "text": [
        "En gloria sublime y santa majestad."
      ]
    },
    "5": {
      "verse": "coro 1",
      "text": [
        "¡Ve adelante, a la victoria, ve!,"
      ]
    },
    "6": {
      "verse": "coro 1",
      "text": [
        "que cielo y tierra tuyos son,",
        "Señor, supremo Rey."
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "Canta, pueblo remanente,",
        "presto salvo serás;"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "pronto fin tendrán tus pruebas",
        "si con el Rey estás."
      ]
    },
    "9": {
      "verse": "coro 2",
      "text": [
        "¡Ve adelante, Señor, supremo Rey!"
      ]
    },
    "10": {
      "verse": "coro 2",
      "text": [
        "En gloria sublime y santa majestad."
      ]
    },
    "11": {
      "verse": "coro 2",
      "text": [
        "¡Ve adelante, a la victoria, ve!,"
      ]
    },
    "12": {
      "verse": "coro 2",
      "text": [
        "que cielo y tierra tuyos son,",
        "Señor, supremo Rey."
      ]
    },
    "13": {
      "verse": "3",
      "text": [
        "Oh, ven pronto, santo día",
        "de esplendor sin igual,"
      ]
    },
    "14": {
      "verse": "3",
      "text": [
        "cuando en hermosura vuelva",
        "Jesús, Rey eternal."
      ]
    },
