{
  "title": "Padre, a tus pies me postro",
  "number": "250",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Padre, a tus pies me postro;",
        "rompe mis prisiones duras;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "oh, responde mientras llamo;",
        "pon tu Espíritu en mí."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Pon tu Espíritu en mi alma;",
        "hazme lo que ser debiera;"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "hazme puro en todo,",
        "libre del pecado;",
        "pon tu Espíritu en mí."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Mientras Cristo intercede,",
        "mientras oro yo humilde,"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "lo que necesito dame;",
        "pon tu Espíritu en mí."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Pon tu Espíritu en mi alma;",
        "hazme lo que ser debiera;"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "hazme puro en todo,",
        "libre del pecado;",
        "pon tu Espíritu en mí."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "No deseo ofenderte,",
        "viviré para agradarte"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "y en el corazón guardarte;",
        "pon tu Espíritu en mí."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Pon tu Espíritu en mi alma;",
        "hazme lo que ser debiera;"
      ]
