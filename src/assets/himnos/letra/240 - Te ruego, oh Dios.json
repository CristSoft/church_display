{
  "title": "Te ruego, oh Dios",
  "number": "240",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Te ruego, oh Dios: ¡Escúchame a mí!,",
        "¡ven, gran Libertador!,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "pues agobiado, anhelo ir a ti.",
        "¡ven, gran Libertador!"
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Por áridas montañas yo vagué,",
        "muy lejos de mi buen Pastor."
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "¡Oh, sálvame y llévame al redil,",
        "ven, gran Libertador!"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "No puedo abrigo en noche oscura hallar,"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "¡ven, gran Libertador!,",
        "mas luz y vida tú me puedes dar,"
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "¡ven, gran Libertador!"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "Por áridas montañas yo vagué,",
        "muy lejos de mi buen Pastor."
      ]
    },
    "9": {
      "verse": "coro",
      "text": [
        "¡Oh, sálvame y llévame al redil,",
        "ven, gran Libertador!"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "Atormentado, sin descanso y paz,",
        "¡ven, gran Libertador!,"
      ]
    },
    "11": {
      "verse": "3",
      "text": [
        "los ojos alzo a tu benigna faz,",
        "¡ven, gran Libertador!"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "Por áridas montañas yo vagué,",
        "muy lejos de mi buen Pastor."
      ]
    },
    "13": {
      "verse": "coro",
      "text": [
        "¡Oh, sálvame y llévame al redil,",
        "ven, gran Libertador!"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "Mi voz contrita tú no desoirás,",
        "¡ven, gran Libertador!,"
      ]
    },
    "15": {
      "verse": "4",
      "text": [
        "mi humilde ruego pronto atenderás,",
        "¡ven, gran Libertador!"
      ]
    },
