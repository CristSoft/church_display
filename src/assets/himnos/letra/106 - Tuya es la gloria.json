{
  "title": "Tuya es la gloria",
  "number": "106",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Tuya es la gloria, Cristo vencedor."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "De la muerte invicto eres, oh Señor."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "Ángeles brillantes vieron tu esplendor,"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "cuando derrotaste muerte y dolor."
      ]
    },
    "5": {
      "verse": "coro 1",
      "text": [
        "Tuya es la gloria,",
        "Cristo vencedor.",
        "De la muerte invicto"
      ]
    },
    "6": {
      "verse": "coro 1",
      "text": [
        "eres, oh Señor."
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "Resucitado, Cristo el Salvador,"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "con amor invita, quita el temor."
      ]
    },
    "9": {
      "verse": "2",
      "text": [
        "Cante hoy la iglesia cántico triunfal;"
      ]
    },
    "10": {
      "verse": "2",
      "text": [
        "Cristo ha vencido al bastión mortal."
      ]
    },
    "11": {
      "verse": "coro 2",
      "text": [
        "Tuya es la gloria,",
        "Cristo vencedor.",
        "De la muerte invicto"
      ]
    },
    "12": {
      "verse": "coro 2",
      "text": [
        "eres, oh Señor."
      ]
    },
    "13": {
      "verse": "3",
      "text": [
        "Ya no dudamos, Príncipe inmortal:"
      ]
    },
    "14": {
      "verse": "3",
      "text": [
        "solo tú das vida, líbranos del mal."
      ]
    },
    "15": {
      "verse": "3",
      "text": [
        "Danos la victoria por tu amor sin par;"
      ]
    },
    "16": {
      "verse": "3",
      "text": [
        "llévanos a salvo al eterno hogar."
      ]
    },
    "17": {
      "verse": "coro 3",
      "text": [
        "Tuya es la gloria,",
        "Cristo vencedor.",
        "De la muerte invicto"
      ]
    },
    "18": {
      "verse": "coro 3",
      "text": [
        "eres, oh Señor."
      ]
    }
  }
