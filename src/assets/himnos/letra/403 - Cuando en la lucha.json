{
  "title": "Cuando en la lucha",
  "number": "403",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Cuando en la lucha me falte poder,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "alguien sabrá, alguien sabrá."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "¿Quién a mis ayes fin puede poner?",
        "alguien sabrá: es Cristo."
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "Alguien sabrá, alguien sabrá;"
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "cuando me oprima y me tiente Satán,"
      ]
    },
    "6": {
      "verse": "coro",
      "text": [
        "Cristo me guardará siempre;",
        "alguien sabrá, es Cristo."
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "Olas de pruebas no las temeré;",
        "alguien sabrá, alguien sabrá;"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "pues cuando mi alma en las sombras esté,"
      ]
    },
    "9": {
      "verse": "2",
      "text": [
        "alguien sabrá: es Cristo."
      ]
    },
    "10": {
      "verse": "coro",
      "text": [
        "Alguien sabrá, alguien sabrá;"
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "cuando me oprima y me tiente Satán,"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "Cristo me guardará siempre;",
        "alguien sabrá, es Cristo."
      ]
    },
    "13": {
      "verse": "3",
      "text": [
        "Débil, herido, abrumado del mal,"
      ]
    },
    "14": {
      "verse": "3",
      "text": [
        "alguien sabrá, alguien sabrá;"
      ]
    },
    "15": {
      "verse": "3",
      "text": [
        "miro con ansia al hogar celestial;",
        "alguien sabrá: es Cristo."
      ]
    },
