{
  "title": "Mi oración",
  "number": "463",
  "verses": "1",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Que mi vida, cada día,",
        "cada paso que dé",
        "sea un cuadro de amor,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "cual Cristo fue.",
        "Y que crezca en Jesús,",
        "que conozca"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "más y más de su amor",
        "y aumente mi fe.",
        "Hasta el día feliz"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "cuando junto a él",
        "pueda siempre vivir",
        "en su gloria:"
      ]
    },
    "5": {
      "verse": "1",
      "text": [
        "es mi oración."
      ]
    }
  }
}