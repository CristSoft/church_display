{
  "title": "Padre amado",
  "number": "368",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Yo te amo más que a todo;",
        "si estoy débil, tú eres fuerte"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "y a mi lado estás.",
        "Te agradezco, Padre amado,"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "porque unes nuestras vidas",
        "a tu corazón."
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "Padre amado,",
        "te agradezco por tus planes,"
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "grandes sueños tienes para mí."
      ]
    },
    "6": {
      "verse": "coro",
      "text": [
        "Tú me guías por las sendas de esta vida,"
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "a tu lado nada faltará."
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "Yo te amo más que a todo;",
        "si estoy débil, tu gran fuerza"
      ]
    },
    "9": {
      "verse": "2",
      "text": [
        "nunca fallará.",
        "Por tu gracia, te imploro"
      ]
    },
    "10": {
      "verse": "2",
      "text": [
        "que transformes hoy mi vida",
        "con tu bendición."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Padre amado,",
        "te agradezco por tus planes,"
      ]
    },
