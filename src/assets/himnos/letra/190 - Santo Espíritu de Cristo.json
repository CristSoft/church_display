{
  "title": "Santo Espíritu de Cristo",
  "number": "190",
  "verses": "4",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Santo Espíritu de Cristo,",
        "mora en este corazón,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "lléname de tu presencia,",
        "cólmame de bendición."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "¡Cólmame! ¡Cólmame!",
        "¡Ven ahora y cólmame!"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "¡Cólmame de tu presencia!",
        "¡Ven, oh ven y cólmame!"
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Santo Espíritu, lo puedes,",
        "aunque cómo, no lo sé;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "mas si tú mis ruegos oyes,",
        "sé que puro yo seré."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "¡Cólmame! ¡Cólmame!",
        "¡Ven ahora y cólmame!"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "¡Cólmame de tu presencia!",
        "¡Ven, oh ven y cólmame!"
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Débil soy, flaqueza todo,",
        "mas me postro a tus pies,"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "para que tu amor eterno,",
        "fuerte, puro y fiel me des."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "¡Cólmame! ¡Cólmame!",
        "¡Ven ahora y cólmame!"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "¡Cólmame de tu presencia!",
        "¡Ven, oh ven y cólmame!"
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "Lávame, bendice y salva",
        "cuerpo, alma, espíritu;"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "ya me salvas, me consuelas,",
        "de bondad me colmas tú."
      ]
    },
