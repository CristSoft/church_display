{
  "title": "Bienvenida da Jesús",
  "number": "217",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Bienvenida da Jesús",
        "en los brazos de su amor"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "al que en busca de la luz",
        "vague ciego y con temor."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Volveremos a cantar:",
        "\"Él recibe al pecador\"."
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "Claro hacedlo resonar:",
        "\"Él recibe al pecador\"."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "A sus pies descansarás;",
        "ejercita en él tu fe;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "de tus males sanarás;",
        "a Jesús, tu amigo, ve."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Volveremos a cantar:",
        "\"Él recibe al pecador\"."
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "Claro hacedlo resonar:",
        "\"Él recibe al pecador\"."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Recibirte prometió,",
        "date prisa en acudir;"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "necesitas como yo vida",
        "que él te hará vivir."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Volveremos a cantar:",
        "\"Él recibe al pecador\"."
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "Claro hacedlo resonar:",
        "\"Él recibe al pecador\"."
      ]
    }
  }
}