{
  "title": "Himno #128 ¡Tu nombre es dulce, buen Jesús!",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "¡Tu nombre es dulce, buen Jesús!"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "¡Oh cuánta paz, consuelo y luz",
        "dimana de tu santa cruz!,"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "mi luz, mi esperanza."
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "¡Dulce nombre: Emanuel!",
        "¡Dulce nombre, siempre fiel!"
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "¡Dulce nombre: gloria a él",
        "los santos todos canten!"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "Jesús; en cuyo corazón",
        "descargo entera mi aflicción,"
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "pues calma toda turbación,",
        "Jesús, tu amado nombre."
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "¡Dulce nombre: Emanuel!",
        "¡Dulce nombre, siempre fiel!"
      ]
    },
    "9": {
      "verse": "coro",
      "text": [
        "¡Dulce nombre: gloria a él",
        "los santos todos canten!"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "Tu nombre pláceme escuchar;",
        "lo siento a mi alma alentar."
      ]
    },
    "11": {
      "verse": "3",
      "text": [
        "Cual canto calma mi llorar,",
        "Jesús, tu santo nombre."
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "¡Dulce nombre: Emanuel!",
        "¡Dulce nombre, siempre fiel!"
      ]
    },
    "13": {
      "verse": "coro",
      "text": [
        "¡Dulce nombre: gloria a él",
        "los santos todos canten!"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "Jamás contó profano autor",
        "cuán dulce el nombre es del Señor;"
      ]
    },
    "15": {
      "verse": "4",
      "text": [
        "ascienda siempre mi loor",
        "a su glorioso nombre."
      ]
    },
    "16": {
      "verse": "coro",
      "text": [
        "¡Dulce nombre: Emanuel!",
        "¡Dulce nombre, siempre fiel!"
      ]
    },
    "17": {
      "verse": "coro",
      "text": [
        "¡Dulce nombre: gloria a él",
        "los santos todos canten!"
      ]
    }
  }
}