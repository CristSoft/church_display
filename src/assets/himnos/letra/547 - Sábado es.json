{
  "title": "Sábado es",
  "number": "547",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Sábado es, sábado es,",
        "día del Señor,",
        "lo hizo Dios por mí."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Me alegraré, me alegraré,",
        "en su gran amor,",
        "en su gran amor."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "Sábado es, día del Señor;",
        "me alegraré",
        "en su gran amor."
      ]
    },
    "4": {
