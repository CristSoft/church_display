{
  "title": "Transfórmame a tu imagen",
  "number": "279",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Hoy pido en oración",
        "que perdones mi tibieza;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "pues quiero, de corazón,",
        "consagrarme a ti, Señor."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Renuévame, Señor;",
        "inspírame valor;",
        "transfórmame a tu imagen;"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "renuévame, Señor."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "A otros quiero llevar",
        "tu mensaje de esperanza;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "viviendo para servir,",
        "compartiendo tu amor."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Renuévame, Señor;",
        "inspírame valor;",
        "transfórmame a tu imagen;"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "renuévame, Señor."
      ]
    }
  }
}