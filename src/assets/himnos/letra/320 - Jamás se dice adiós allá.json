{
  "title": "Jamás se dice \"adiós\" allá",
  "number": "320",
  "verses": "3",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Cuán grato es con amigos vernos",
        "en tiempo tan veloz;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "mas siempre llega el día triste",
        "en que se dice \"adiós\"."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Jamás se dice \"adiós\" allá,",
        "jamás se dice \"adiós\";"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "en el país de gozo y paz",
        "jamás se dice \"adiós\"."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Cuán dulce es el consuelo dado",
        "por ellos al partir:"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "que cuando venga Cristo en gloria,",
        "nos hemos de reunir."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Jamás se dice \"adiós\" allá,",
        "jamás se dice \"adiós\";"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "en el país de gozo y paz",
        "jamás se dice \"adiós\"."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Jamás habrá una despedida,",
        "tan dolorosa aquí;"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "mas grata unión y paz eterna",
        "se gozarán allí."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Jamás se dice \"adiós\" allá,",
        "jamás se dice \"adiós\";"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "en el país de gozo y paz",
        "jamás se dice \"adiós\"."
      ]
    }
  }
}