{
  "title": "Himno #173 ¿Será al albor?",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "¿Será al albor, cuando el día despierta"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "y el sol cada sombra nocturna ahuyenta?"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "¿Será al albor cuando en toda su gloria"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "volverá a la tierra Jesús?"
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "¿Cuánto aún faltará, Señor,",
        "hasta que cante así:"
      ]
    },
    "6": {
      "verse": "coro",
      "text": [
        "\"Cristo vuelve, ¡aleluya! ¡aleluya! Amén.",
        "¡Aleluya! Amén\"?"
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "Trompeta triunfal todos pronto oiremos;"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "su escolta de ángeles santos veremos;"
      ]
    },
    "9": {
      "verse": "2",
      "text": [
        "su sien coronada de luz miraremos"
      ]
    },
    "10": {
      "verse": "2",
      "text": [
        "cuando venga en gloria Jesús."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "¿Cuánto aún faltará, Señor,",
        "hasta que cante así:"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "\"Cristo vuelve, ¡aleluya! ¡aleluya! Amén.",
        "¡Aleluya! Amén\"?"
      ]
    },
    "13": {
      "verse": "3",
      "text": [
        "¡Qué gozo habrá cuando acabe la muerte!"
      ]
    },
    "14": {
      "verse": "3",
      "text": [
        "Vivir sin dolor tocaranos en suerte;"
      ]
    },
    "15": {
      "verse": "3",
      "text": [
        "las penas y el mal quitará el Dios fuerte"
      ]
    },
    "16": {
      "verse": "3",
      "text": [
        "cuando venga en gloria Jesús."
      ]
    },
    "17": {
      "verse": "coro",
      "text": [
        "¿Cuánto aún faltará, Señor,",
        "hasta que cante así:"
      ]
    },
    "18": {
      "verse": "coro",
      "text": [
        "\"Cristo vuelve, ¡aleluya! ¡aleluya! Amén.",
        "¡Aleluya! Amén\"?"
      ]
    }
  }
}