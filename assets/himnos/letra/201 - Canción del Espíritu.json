{
  "title": "Himno #201 Canción del Espíritu",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Oh, deja que el Señor te envuelva",
        "con su Espíritu de amor;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "satisfaga hoy tu alma y corazón."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "Entrégale lo que él te indique,",
        "y su Espíritu vendrá"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "sobre ti y vida nueva te dará."
      ]
    },
    "5": {
      "verse": "coro",
      "text": [
        "Cristo, ¡oh, Cristo!",
        "Ven y llénanos.",
        "Cristo, ¡oh, Cristo!"
      ]
    },
    "6": {
      "verse": "coro",
      "text": [
        "Llénanos de ti."
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "Alzamos nuestra voz con gozo,",
        "nuestra alabanza a ti;"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "con dulzura te entregamos nuestro ser."
      ]
    },
    "9": {
      "verse": "2",
      "text": [
        "Entrega toda tu tristeza",
        "en el nombre de Jesús,"
      ]
    },
    "10": {
      "verse": "2",
      "text": [
        "y abundante vida hoy tendrás en él."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Cristo, ¡oh, Cristo!",
        "Ven y llénanos.",
        "Cristo, ¡oh, Cristo!"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "Llénanos de ti."
      ]
    }
  }
}