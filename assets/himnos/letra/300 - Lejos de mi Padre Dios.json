{
  "title": "Himno #300 Lejos de mi Padre Dios",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Lejos de mi Padre Dios,",
        "por Jesús hallado,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "por su gracia y por su amor",
        "solo fui salvado."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "En Jesús, mi Señor, es mi gloria eterna;"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "él me amó y me salvó en su gracia tierna."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "En Jesús, mi Salvador,",
        "pongo mi confianza;",
        "toda mi necesidad"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "suple en abundancia."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "En Jesús, mi Señor, es mi gloria eterna;"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "él me amó y me salvó en su gracia tierna."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Cerca de mi buen Pastor",
        "vivo cada día;"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "toda gracia en ti, Señor,",
        "halla el alma mía."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "En Jesús, mi Señor, es mi gloria eterna;"
      ]
    },
    "12": {
      "verse": "coro",
      "text": [
        "él me amó y me salvó en su gracia tierna."
      ]
    },
    "13": {
      "verse": "4",
      "text": [
        "Guárdame, Señor Jesús,",
        "para que no caiga;"
      ]
    },
    "14": {
      "verse": "4",
      "text": [
        "cual sarmiento de la vid,",
        "vida de ti traiga."
      ]
    },
