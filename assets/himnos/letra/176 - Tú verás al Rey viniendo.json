{
  "title": "Himno #176 Tú verás al Rey viniendo",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Tú verás al Rey viniendo,",
        "y oirás son de trompetas."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Tú verás al Rey viniendo,",
        "falta poco ya."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Hoy escucha el coro,",
        "oye las trompetas.",
        "Hoy escucha el coro"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "que anuncia al Señor."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Resucitarán los justos,",
        "miles de ángeles los llevan"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "a los pies de Jesucristo;",
        "falta poco ya."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Hoy escucha el coro,",
        "oye las trompetas.",
        "Hoy escucha el coro"
