{
  "title": "Himno #148 Solo Cristo",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Solo Cristo, solo él",
        "me redime, me da paz.",
        "El anhelo de mi ser"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "tan solo Cristo suplirá."
      ]
    }
  }
}