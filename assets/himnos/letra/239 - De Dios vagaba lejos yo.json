{
  "title": "Himno #239 De Dios vagaba lejos yo",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "De Dios vagaba lejos yo,",
        "vuelvo hoy a ti;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "por sendas donde el mal reinó,",
        "vuelvo hoy a ti."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Ya no más, oh Señor,",
        "voy errando así;",
        "a los brazos de tu amor,"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "Cristo, vuelvo, oh sí."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Cansado de pecar estoy,",
        "vuelvo hoy a ti;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "en ti espero desde hoy,",
        "vuelvo hoy a ti."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Ya no más, oh Señor,",
        "voy errando así;",
        "a los brazos de tu amor,"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "Cristo, vuelvo, oh sí."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Confío solo en esto yo,",
        "vuelvo hoy a ti:"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "en que Jesús por mí murió;",
        "vuelvo hoy a ti."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Ya no más, oh Señor,",
