{
  "title": "Himno #89 Cristianos, alegraos hoy",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Cristianos, alegraos hoy",
        "con alma, corazón y voz."
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "Ha nacido hoy Jesús,",
        "presten todos atención."
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "Buey y asno acompañan",
        "al bebé en el mesón.",
        "Cristo ya nació,"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "Cristo ya nació."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Cristianos, alegraos hoy",
        "con alma, corazón y voz."
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "Escuchad la bendición,",
        "fue por esto que él nació."
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "Las ventanas de los cielos",
        "al nacer él las abrió."
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "Cristo ya nació,",
        "Cristo ya nació."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Cristianos, alegraos hoy",
        "con alma, corazón y voz."
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "No más miedo a morir:",
        "él nos vino a salvar."
      ]
    },
    "11": {
      "verse": "3",
      "text": [
        "Él nos llama a todos hoy,",
        "la vida eterna quiere dar."
      ]
    },
    "12": {
      "verse": "3",
      "text": [
        "Cristo ya nació,",
        "Cristo ya nació."
      ]
    }
  }
}