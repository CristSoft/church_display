{
  "title": "Himno #287 Rey de mi vida",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Rey de mi vida tú eres hoy;",
        "en ti me gloriaré;"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "yo por tu cruz salvado soy:",
        "no te olvidaré."
      ]
    },
    "3": {
      "verse": "coro",
      "text": [
        "Después de tu Getsemaní,",
        "subiste a la cruz más cruel;"
      ]
    },
    "4": {
      "verse": "coro",
      "text": [
        "todo sufrió tu amor por mí:",
        "yo quiero serte fiel."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "Mas vi la luz amanecer",
        "de la eternidad;",
        "te vi, Señor, aparecer"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "con inmortalidad."
      ]
    },
    "7": {
      "verse": "coro",
      "text": [
        "Después de tu Getsemaní,",
        "subiste a la cruz más cruel;"
      ]
    },
    "8": {
      "verse": "coro",
      "text": [
        "todo sufrió tu amor por mí:",
        "yo quiero serte fiel."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Rey de mi vida, Rey de luz,",
        "en ti me gloriaré;"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "por mí moriste en la cruz:",
        "no te olvidaré."
      ]
    },
    "11": {
      "verse": "coro",
      "text": [
        "Después de tu Getsemaní,",
        "subiste a la cruz más cruel;"
      ]
    },
    "12": {
