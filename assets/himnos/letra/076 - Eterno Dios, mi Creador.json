{
  "title": "Himno #76 Eterno Dios, mi Creador",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Eterno Dios, mi Creador,",
        "mi amparo en aflicción,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "tú has sido mi Consolador",
        "en toda ocasión."
      ]
    },
    "3": {
      "verse": "2",
      "text": [
        "Mil años a tu vista son",
        "cual brisas del ayer;"
      ]
    },
    "4": {
      "verse": "2",
      "text": [
        "cual hierba es mi condición,",
        "perece atardecer."
      ]
    },
    "5": {
      "verse": "3",
      "text": [
        "Mi vida bajo tu ala está,",
        "seguro habitaré;"
      ]
    },
    "6": {
      "verse": "3",
      "text": [
        "tu Espíritu me ayudará",
        "y en calma andaré."
      ]
    },
    "7": {
      "verse": "4",
      "text": [
        "Eterno Dios, mi Redentor,",
        "confío solo en ti;"
      ]
    },
    "8": {
      "verse": "4",
      "text": [
        "sé tú mi Guía, oh Señor,",
        "en mi camino aquí."
      ]
    }
  }
}