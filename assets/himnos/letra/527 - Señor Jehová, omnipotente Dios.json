{
  "title": "Himno #527 Señor Jehová, omnipotente Dios",
  "sections": {
    "1": {
      "verse": "1",
      "text": [
        "Señor Jehová, omnipotente Dios,"
      ]
    },
    "2": {
      "verse": "1",
      "text": [
        "tú que los astros riges con poder,"
      ]
    },
    "3": {
      "verse": "1",
      "text": [
        "oye clemente nuestra humilde voz,"
      ]
    },
    "4": {
      "verse": "1",
      "text": [
        "nuestra oración hoy puedas responder."
      ]
    },
    "5": {
      "verse": "2",
      "text": [
        "A nuestra patria da tu bendición;"
      ]
    },
    "6": {
      "verse": "2",
      "text": [
        "enséñanos tus leyes a guardar;"
      ]
    },
    "7": {
      "verse": "2",
      "text": [
        "alumbra la conciencia y la razón,"
      ]
    },
    "8": {
      "verse": "2",
      "text": [
        "y reina siempre tú en todo hogar."
      ]
    },
    "9": {
      "verse": "3",
      "text": [
        "Danos valor al enfrentar el mal;"
      ]
    },
    "10": {
      "verse": "3",
      "text": [
        "condúcenos por sendas de virtud."
      ]
    },
    "11": {
      "verse": "3",
      "text": [
        "Protégenos con mano paternal,"
      ]
    },
    "12": {
      "verse": "3",
      "text": [
        "concédenos tu paz, consuelo y luz."
      ]
    }
  }
}